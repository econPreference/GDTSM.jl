\begin_layout Subsection
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

 is non-diagonal
\end_layout

\begin_layout Standard
Our market prices of risks specification 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:mpr"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

 is 
\begin_inset Formula 
\begin{align*}
\lambda_{t} & =\Omega_{\mathcal{FF}}^{-1/2}[(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})+\sum_{l=1}^{p}(G_{\mathcal{FF},l}^{\mathbb{P}}-G_{\mathcal{FF},l}^{\mathbb{Q}})\mathcal{F}_{t+1-l}].
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
We first derive the conditional posterior distribution of 
\begin_inset Formula $\{\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}},\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}}\}$
\end_inset

 using a likelihood of equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:structural_VAR"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\underbrace{\begin{pmatrix}1 & 0 & 0 & \cdots & 0\\
\mathcal{C}_{21} & 1 & 0 & \cdots & 0\\
\mathcal{C}_{31} & \mathcal{C}_{32} & 1 & \cdots & 0\\
\vdots & \vdots & \vdots & \ddots & \vdots\\
\mathcal{C}_{d_{\mathbb{P}}1} & \mathcal{C}_{d_{\mathbb{P}}2} & \mathcal{C}_{d_{\mathbb{P}}3} & \cdots & 1
\end{pmatrix}}_{\mathcal{C}}\mathcal{F}_{t}=\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t-l}+diag(\underbrace{\begin{bmatrix}\sigma_{\mathcal{FF},1}\\
\vdots\\
\sigma_{\mathcal{FF},d_{\mathbb{P}}}
\end{bmatrix}}_{\tilde{\Omega}_{\mathcal{FF}}^{1/2}})v_{\mathcal{F},t}^{\mathbb{P}}.
\]

\end_inset

Since we know 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

 and 
\begin_inset Formula $\{\mathcal{F}_{t}\}_{t=1}^{T}$
\end_inset

,
 
\begin_inset Formula $\{\mathcal{C}\mathcal{F}_{t}\}_{t=1}^{T}$
\end_inset

 and 
\begin_inset Formula $\{\sigma_{\mathcal{FF},i}\}_{i=1}^{d_{\mathbb{P}}}$
\end_inset

 are also known.
 Since the above system has a diagonal covariance matrix,
 parameters in
\begin_inset Formula 
\[
\begin{bmatrix}\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}} & \mathcal{C}G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}_{(i,:)}\equiv\begin{bmatrix}\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}} & \mathcal{C}G_{\mathcal{FF},1}^{\mathbb{P}} & \cdots & \mathcal{C}G_{\mathcal{FF},p}^{\mathbb{P}}\end{bmatrix}_{(i,:)}
\]

\end_inset

are posteriorly independent across 
\begin_inset Formula $i$
\end_inset

.
 By Normal-Normal update,
 we can derive the conditional posterior distribution as
\begin_inset Formula 
\[
p(\begin{bmatrix}\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}} & \mathcal{C}G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}_{(i,:)}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}})\backsim\mathcal{N}(\beta_{CKCG,i},B_{CKCG,i}),
\]

\end_inset

where
\begin_inset Formula 
\begin{align*}
B_{CKCG,i} & \equiv(diag[V_{i,(1:1+d_{\mathbb{P}}p)}]^{-1}+\sigma_{\mathcal{FF},i}^{-2}X_{\phi_{i},(1:1+d_{\mathbb{P}}p)}^{\prime}X_{\phi_{i},(1:1+d_{\mathbb{P}}p)})^{-1},\\
\beta_{CKCG,i} & \equiv B_{CKCG,i}(diag[V_{i,(1:1+d_{\mathbb{P}}p)}]^{-1}m_{i,(1:1+d_{\mathbb{P}}p)}+\sigma_{\mathcal{FF},i}^{-2}X_{\phi_{i},(1:1+d_{\mathbb{P}}p)}^{\prime}\begin{bmatrix}(\mathcal{C}\mathcal{F}_{1})_{(i)}\\
\vdots\\
(\mathcal{C}\mathcal{F}_{T})_{(i)}
\end{bmatrix}).
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
And then,
 we derive the posterior distribution of the reduced form parameters.
 To do that,
 we check the relationship between two types of parameters,
\begin_inset Formula 
\begin{align*}
vec\left[\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}} & G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}^{\prime}\right] & =vec\left[\begin{bmatrix}\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}} & \mathcal{C}G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}^{\prime}\mathcal{C}^{-1\prime}\right],\\
 & =(\mathcal{C}^{-1}\otimes I_{1+d_{\mathbb{P}}p})vec\left[\begin{bmatrix}\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}} & \mathcal{C}G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}^{\prime}\right],
\end{align*}

\end_inset

where 
\begin_inset Formula $G_{\mathcal{FF}}^{\mathbb{P}}\equiv\begin{pmatrix}G_{\mathcal{FF},1}^{\mathbb{P}} & \cdots & G_{\mathcal{FF},p}^{\mathbb{P}}\end{pmatrix}$
\end_inset

.
 The distribution of 
\begin_inset Formula $vec\left[\begin{bmatrix}\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}} & \mathcal{C}G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}^{\prime}\right]$
\end_inset

 conditional on 
\begin_inset Formula $\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}$
\end_inset

 is 
\begin_inset Formula 
\[
\mathcal{N}\left(\beta_{CKCG}\equiv\begin{bmatrix}\beta_{CKCG,1}\\
\vdots\\
\beta_{CKCG,d_{\mathbb{P}}}
\end{bmatrix},B_{CKCG}\equiv\begin{pmatrix}B_{CKCG,1} & \cdots & O_{(d_{\mathbb{P}}p+1)\times(d_{\mathbb{P}}p+1)}\\
\vdots & \ddots & \vdots\\
O_{(d_{\mathbb{P}}p+1)\times(d_{\mathbb{P}}p+1)} & \cdots & B_{CKCG,d_{\mathbb{P}}}
\end{pmatrix}\right).
\]

\end_inset

Therefore,
 the distribution of 
\begin_inset Formula $vec\left[\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}} & G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}^{\prime}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}\right]$
\end_inset

 is 
\begin_inset Formula 
\[
\mathcal{N}\left(\beta_{KG}\equiv(\mathcal{C}^{-1}\otimes I_{1+d_{\mathbb{P}}p})\beta_{CKCG},B_{KG}\equiv(\mathcal{C}^{-1}\otimes I_{1+d_{\mathbb{P}}p})B_{CKCG}(\mathcal{C}^{-1\prime}\otimes I_{1+d_{\mathbb{P}}p})\right).
\]

\end_inset


\end_layout

\begin_layout Standard
Next,
 by the definition of the 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics,
\begin_inset Formula 
\begin{align*}
 & vec\left[\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}} & G_{\mathcal{FF}}^{\mathbb{P}}-G_{\mathcal{FF}}^{\mathbb{Q}}\end{bmatrix}^{\prime}\right]\\
= & vec\left[\begin{bmatrix}K_{\mathcal{P}}^{\mathbb{P}}-K_{\mathcal{P}}^{\mathbb{Q}} & G_{\mathcal{PP},1}^{\mathbb{P}}-G_{\mathcal{PP}}^{\mathbb{Q}} & G_{\mathcal{PM},1}^{\mathbb{P}} & \cdots & G_{\mathcal{PP},p}^{\mathbb{P}} & G_{\mathcal{PM},p}^{\mathbb{P}}\\
 &  & O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times(d_{\mathbb{P}}p+1)}
\end{bmatrix}^{\prime}\right].
\end{align*}

\end_inset

Therefore,
 it has a degenerated distribution.
 The degenerated distribution is 
\begin_inset Formula 
\begin{align*}
 & \begin{bmatrix}\mathcal{N}\left(\beta_{KG,(1:d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p)}-vec\left[\begin{bmatrix}K_{\mathcal{P}}^{\mathbb{Q}} & G_{\mathcal{PP}}^{\mathbb{Q}} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}p-d_{\mathbb{Q}})}\end{bmatrix}^{\prime}\right],B_{KG,(1:d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p,1:d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p)}\right)\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})(d_{\mathbb{P}}p+1)\times1}
\end{bmatrix}\\
= & \mathcal{N}\left(\beta_{KGPQ}\equiv\begin{bmatrix}\beta_{KG,(1:d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p)}-vec\left[\begin{bmatrix}K_{\mathcal{P}}^{\mathbb{Q}} & G_{\mathcal{PP}}^{\mathbb{Q}} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}p-d_{\mathbb{Q}})}\end{bmatrix}^{\prime}\right]\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})(d_{\mathbb{P}}p+1)\times1}
\end{bmatrix},\right.\\
 & \left.B_{KGPQ}\equiv\begin{pmatrix}B_{KG,(1:d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p,1:d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p)} & O_{(d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p)\times(d_{\mathbb{P}}-d_{\mathbb{Q}})(d_{\mathbb{P}}p+1)}\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})(d_{\mathbb{P}}p+1)\times(d_{\mathbb{Q}}+d_{\mathbb{Q}}d_{\mathbb{P}}p)} & O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})(d_{\mathbb{P}}p+1)\times(d_{\mathbb{P}}-d_{\mathbb{Q}})(d_{\mathbb{P}}p+1)}
\end{pmatrix}\right).
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Given 
\begin_inset Formula $\{\mathcal{F}_{t}\}_{t=1}^{T}$
\end_inset

,
 since
\begin_inset Formula 
\[
(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})+\sum_{l=1}^{p}(G_{\mathcal{FF},l}^{\mathbb{P}}-G_{\mathcal{FF},l}^{\mathbb{Q}})\mathcal{F}_{t+1-l}=\underbrace{\left(I_{d_{\mathbb{P}}}\otimes\begin{bmatrix}1\\
\mathcal{F}_{t}\\
\vdots\\
\mathcal{F}_{t+1-p}
\end{bmatrix}^{\prime}\right)}_{\mathcal{F}_{kron,t}}vec\left[\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}} & G_{\mathcal{FF}}^{\mathbb{P}}-G_{\mathcal{FF}}^{\mathbb{Q}}\end{bmatrix}^{\prime}\right],
\]

\end_inset

the distribution of 
\begin_inset Formula $(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})+\sum_{l=1}^{p}(G_{\mathcal{FF},l}^{\mathbb{P}}-G_{\mathcal{FF},l}^{\mathbb{Q}})\mathcal{F}_{t+1-l}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}$
\end_inset

 is 
\begin_inset Formula 
\[
\mathcal{N}(\mathcal{F}_{kron,t}\beta_{KGPQ},\mathcal{F}_{kron,t}B_{KGPQ}\mathcal{F}_{kron,t}').
\]

\end_inset

Therefore,
 the distribution of 
\begin_inset Formula $\lambda_{t}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}$
\end_inset

 is 
\begin_inset Formula 
\[
\mathcal{N}\left(\beta_{\lambda_{t}}\equiv\Omega_{\mathcal{FF}}^{-1/2}\mathcal{F}_{kron,t}\beta_{KGPQ},B_{\lambda_{t}}\equiv(\Omega_{\mathcal{FF}}^{-1/2}\mathcal{F}_{kron,t})B_{KGPQ}(\Omega_{\mathcal{FF}}^{-1/2}\mathcal{F}_{kron,t})^{\prime}\right).
\]

\end_inset


\end_layout

\begin_layout Standard
We want to derive 
\begin_inset Formula $\mathbb{E}[\sqrt{\lambda_{t}'\lambda_{t}}]$
\end_inset

.
 By the second-order Taylor approximation,
 
\begin_inset Formula 
\begin{align*}
\mathbb{E}[\sqrt{\lambda_{t}'\lambda_{t}}] & =\mathbb{E}[\sum_{n=0}^{\infty}\frac{\sqrt{\pi}}{2}\frac{\mathbb{E}[\lambda_{t}'\lambda_{t}]^{0.5-n}(\lambda_{t}'\lambda_{t}-\mathbb{E}[\lambda_{t}'\lambda_{t}])^{n}}{\Gamma(1.5-n)n!}],\\
 & \approx\frac{\sqrt{\pi}}{2}\frac{\mathbb{E}[\lambda_{t}'\lambda_{t}]^{0.5}}{\Gamma(1.5)}+\frac{\sqrt{\pi}}{2}\frac{\mathbb{E}[\lambda_{t}'\lambda_{t}]^{-1.5}Var[\lambda_{t}'\lambda_{t}]}{2\Gamma(-0.5)}.
\end{align*}

\end_inset

By the formula for the quadratic form in statistics,
 
\begin_inset Formula 
\begin{align*}
\mathbb{E}[\lambda_{t}'\lambda_{t}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}] & =tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}},\\
Var[\lambda_{t}'\lambda_{t}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}] & =2tr[B_{\lambda_{t}}^{2}]+4\beta_{\lambda_{t}}^{\prime}B_{\lambda_{t}}\beta_{\lambda_{t}}.
\end{align*}

\end_inset

Therefore,
 we can calculate
\begin_inset Formula 
\begin{align*}
 & \mathbb{E}[\sqrt{\lambda_{t}'\lambda_{t}}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}]\\
\approx & \frac{\sqrt{\pi}}{2}\left[\frac{(tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}})^{0.5}}{\Gamma(1.5)}+\frac{(tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}})^{-1.5}(2tr[B_{\lambda_{t}}^{2}]+4\beta_{\lambda_{t}}^{\prime}B_{\lambda_{t}}\beta_{\lambda_{t}})}{2\Gamma(-0.5)}\right].
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

 is a diagonal
\end_layout

\begin_layout Standard
If 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

 is a diagonal,
 
\begin_inset Formula 
\begin{align*}
\Omega_{\mathcal{FF}}^{1/2} & =diag[\tilde{\Omega}_{\mathcal{FF}}^{1/2}].
\end{align*}

\end_inset

and 
\begin_inset Formula 
\begin{align*}
\lambda_{t} & =diag[\tilde{\Omega}_{\mathcal{FF}}^{-1/2}][(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})+\sum_{l=1}^{p}(G_{\mathcal{FF},l}^{\mathbb{P}}-G_{\mathcal{FF},l}^{\mathbb{Q}})\mathcal{F}_{t+1-l}].
\end{align*}

\end_inset

Also,
 since 
\begin_inset Formula $\mathcal{C}$
\end_inset

 is an identity matrix,
 the priors between 
\begin_inset Formula $\{K_{\mathcal{F}}^{\mathbb{P}},G_{\mathcal{FF}}^{\mathbb{P}}\}$
\end_inset

 and 
\begin_inset Formula $\{\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}},\mathcal{C}G_{\mathcal{FF}}^{\mathbb{P}}\}$
\end_inset

 are the same.
 Therefore,
 we can use a likelihood of equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:structural_VAR"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{F}_{t}=K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t-l}+diag(\underbrace{\begin{bmatrix}\sigma_{\mathcal{FF},1}\\
\vdots\\
\sigma_{\mathcal{FF},d_{\mathbb{P}}}
\end{bmatrix}}_{\tilde{\Omega}_{\mathcal{FF}}^{1/2}})v_{\mathcal{F},t}^{\mathbb{P}}
\]

\end_inset

to use Normal-Normal update.
 Specifically,
 we can derive the conditional posterior distribution as
\begin_inset Formula 
\[
p(\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}} & G_{\mathcal{FF}}^{\mathbb{P}}\end{bmatrix}_{(i,:)}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}})\backsim\mathcal{N}(\beta_{CKCG,i},B_{CKCG,i}),
\]

\end_inset

where
\begin_inset Formula 
\begin{align*}
B_{CKCG,i} & \equiv(diag[V_{i,(1:1+d_{\mathbb{P}}p)}]^{-1}+\sigma_{\mathcal{FF},i}^{-2}X_{\phi_{i},(:,1:1+d_{\mathbb{P}}p)}^{\prime}X_{\phi_{i},(:,1:1+d_{\mathbb{P}}p)})^{-1},\\
\beta_{CKCG,i} & \equiv B_{CKCG,i}(diag[V_{i,(1:1+d_{\mathbb{P}}p)}]^{-1}m_{i,(1:1+d_{\mathbb{P}}p)}+\sigma_{\mathcal{FF},i}^{-2}X_{\phi_{i},(:,1:1+d_{\mathbb{P}}p)}^{\prime}\begin{bmatrix}\mathcal{F}_{1,(i)}\\
\vdots\\
\mathcal{F}_{T,(i)}
\end{bmatrix}).
\end{align*}

\end_inset

Because of the definition of 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics,
 
\begin_inset Formula 
\begin{align*}
 & p(\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}} & G_{\mathcal{FF}}^{\mathbb{P}}-G_{\mathcal{FF}}^{\mathbb{Q}}\end{bmatrix}_{(i,:)}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}})\\
\backsim & \mathcal{N}(\beta_{KGPQ,i}\equiv\beta_{CKCG,i}-\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{Q}} & G_{\mathcal{FF}}^{\mathbb{Q}}\end{bmatrix}_{(i,:)},B_{KGPQ,i}\equiv B_{CKCG,i})\text{ for \ensuremath{i\leq d_{\mathbb{Q}},}}
\end{align*}

\end_inset

and for 
\begin_inset Formula $i>d_{\mathbb{Q}}$
\end_inset

 the distribution 
\begin_inset Formula $\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}} & G_{\mathcal{FF}}^{\mathbb{P}}-G_{\mathcal{FF}}^{\mathbb{Q}}\end{bmatrix}_{(i,:)}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}$
\end_inset

 degenerates to zero vector.
 Then,
 
\begin_inset Formula 
\begin{align*}
 & [(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})+\sum_{l=1}^{p}(G_{\mathcal{FF},l}^{\mathbb{P}}-G_{\mathcal{FF},l}^{\mathbb{Q}})\mathcal{F}_{t+1-l}]_{(i,:)}\\
= & \begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}} & G_{\mathcal{FF}}^{\mathbb{P}}-G_{\mathcal{FF}}^{\mathbb{Q}}\end{bmatrix}_{(i,:)}\underbrace{\begin{bmatrix}1\\
\mathcal{F}_{t}\\
\vdots\\
\mathcal{F}_{t+1-p}
\end{bmatrix}}_{\mathcal{F}_{const,t}}\\
\backsim & \mathcal{N}(\beta_{KGPQ,i}\mathcal{F}_{const,t},\mathcal{F}_{const,t}^{\prime}B_{KGPQ,i}\mathcal{F}_{const,t})\text{ }\text{for }\ensuremath{i\leq d_{\mathbb{Q}},}
\end{align*}

\end_inset

or zero,
 otherwise.
 Finally,
 
\begin_inset Formula 
\begin{align*}
\lambda_{t,(i)} & =\mathcal{N}(\beta_{\lambda_{t,(i)}}\equiv\sigma_{\mathcal{FF},i}^{-1}\beta_{KGPQ,i}\mathcal{F}_{const,t},B_{\lambda_{t,(i)}}\equiv\sigma_{\mathcal{FF},i}^{-2}\mathcal{F}_{const,t}^{\prime}B_{KGPQ,i}\mathcal{F}_{const,t})\text{ }\text{for }\ensuremath{i\leq d_{\mathbb{Q}},}
\end{align*}

\end_inset

or zero,
 otherwise.
 Since,
 the last elements are zeros,
 
\begin_inset Formula 
\begin{align*}
\lambda_{t}^{\prime}\lambda_{t} & =\lambda_{t,(1:d_{\mathbb{Q}})}^{\prime}\lambda_{t,(1:d_{\mathbb{Q}})}.
\end{align*}

\end_inset

Therefore,
 it is enough to focus on 
\begin_inset Formula $\lambda_{t,(1:d_{\mathbb{Q}})}$
\end_inset

.
 The distribution of 
\begin_inset Formula $\lambda_{t,(1:d_{\mathbb{Q}})}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}$
\end_inset

 is
\begin_inset Formula 
\begin{align*}
 & \mathcal{N}\left(\beta_{\lambda_{t}}\equiv\begin{bmatrix}\beta_{\lambda_{t,(1)}}\\
\vdots\\
\beta_{\lambda_{t,(d_{\mathbb{Q}})}}
\end{bmatrix},B_{\lambda_{t}}\equiv diag\begin{bmatrix}B_{\lambda_{t,(1)}}\\
\vdots\\
B_{\lambda_{t,(d_{\mathbb{Q}})}}
\end{bmatrix}\right).
\end{align*}

\end_inset

Using the above result,
 we can derive 
\begin_inset Formula 
\begin{align*}
 & \mathbb{E}[\sqrt{\lambda_{t}'\lambda_{t}}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}]\\
\approx & \frac{\sqrt{\pi}}{2}\left[\frac{(tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}})^{0.5}}{\Gamma(1.5)}+\frac{(tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}})^{-1.5}(2tr[B_{\lambda_{t}}^{2}]+4\beta_{\lambda_{t}}^{\prime}B_{\lambda_{t}}\beta_{\lambda_{t}})}{2\Gamma(-0.5)}\right].
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
For deriving the time invariant part 
\begin_inset Formula 
\begin{align*}
 & \sqrt{\lambda_{t,const}'\lambda_{t,const}},\\
\text{where }\lambda_{t,const} & :=diag[\tilde{\Omega}_{\mathcal{FF}}^{-1/2}](K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}}),
\end{align*}

\end_inset

first check that 
\begin_inset Formula 
\begin{align*}
 & p(\begin{bmatrix}K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}}\end{bmatrix}_{(i,:)}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}})\\
\backsim & \mathcal{N}(\beta_{KGPQ,i,(1)},B_{KGPQ,i,(1,1)})\text{ for \ensuremath{i\leq d_{\mathbb{Q}},}}
\end{align*}

\end_inset

and zero,
 otherwise.
 Therefore,
 
\begin_inset Formula 
\begin{align*}
 & p(\lambda_{t,const,(i)}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}})\\
\backsim & \mathcal{N}(\sigma_{\mathcal{FF},i}^{-1}\beta_{KGPQ,i,(1)},\sigma_{\mathcal{FF},i}^{-2}B_{KGPQ,i,(1,1)})\text{ for \ensuremath{i\leq d_{\mathbb{Q}},}}
\end{align*}

\end_inset

and zero,
 otherwise.
 Similarly,
 since the last elements are zeros,
 
\begin_inset Formula 
\[
\lambda_{t,const}^{\prime}\lambda_{t,const}=\lambda_{t,const,(1:d_{\mathbb{Q}})}^{\prime}\lambda_{t,const,(1:d_{\mathbb{Q}})}.
\]

\end_inset

With the same logic,
 since the distribution of 
\begin_inset Formula $\lambda_{t,const,(1:d_{\mathbb{Q}})}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}$
\end_inset

 is 
\begin_inset Formula 
\begin{align*}
 & \mathcal{N}\left(\beta_{const}\equiv\begin{bmatrix}\sigma_{\mathcal{FF},1}^{-1}\beta_{KGPQ,1,(1)}\\
\vdots\\
\sigma_{\mathcal{FF},d_{\mathbb{Q}}}^{-1}\beta_{KGPQ,d_{\mathbb{Q}},(1)}
\end{bmatrix},B_{const}\equiv diag\begin{bmatrix}\sigma_{\mathcal{FF},1}^{-2}B_{KGPQ,1,(1,1)}\\
\vdots\\
\sigma_{\mathcal{FF},d_{\mathbb{Q}}}^{-2}B_{KGPQ,d_{\mathbb{Q}},(1,1)}
\end{bmatrix}\right),
\end{align*}

\end_inset

the approximation is 
\begin_inset Formula 
\begin{align*}
 & \mathbb{E}[\sqrt{\lambda_{t,const}^{\prime}\lambda_{t,const}}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}]\\
\approx & \frac{\sqrt{\pi}}{2}\left[\frac{(tr[B_{const}]+\beta_{const}^{\prime}\beta_{\lambda})^{0.5}}{\Gamma(1.5)}+\frac{(tr[B_{const}]+\beta_{const}^{\prime}\beta_{const})^{-1.5}(2tr[B_{const}^{2}]+4\beta_{const}^{\prime}B_{const}\beta_{const})}{2\Gamma(-0.5)}\right].
\end{align*}

\end_inset


\end_layout

