\begin_layout Section
Bond pricing model
\end_layout

\begin_layout Subsection
JSZ normalization
\end_layout

\begin_layout Standard
The short-term interest rate 
\begin_inset Formula $r_{t}$
\end_inset

 is 
\begin_inset Formula 
\begin{align}
\underset{(1\times1)}{r_{t}} & =\underset{(1\times d_{\mathbb{Q}})}{\iota^{\prime}}\underset{(d_{\mathbb{Q}}\times1)}{X_{t}},\label{eq:r}
\end{align}

\end_inset

where 
\begin_inset Formula $\iota$
\end_inset

 is a vector of ones.
 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics is 
\begin_inset Formula 
\begin{align}
\underset{(d_{\mathbb{Q}}\times1)}{X_{t}} & =\underset{(d_{\mathbb{Q}}\times1)}{K_{X}^{\mathbb{Q}}}+\underset{(d_{\mathbb{Q}}\times d_{\mathbb{Q}})}{G_{XX}^{\mathbb{Q}}}X_{t-1}+\underset{(d_{\mathbb{Q}}\times1)}{\epsilon_{X,t}^{\mathbb{Q}}},\label{eq:Q_X}\\
K_{X}^{\mathbb{Q}} & \equiv\begin{bmatrix}\underset{(1\times1)}{k_{\infty}^{\mathbb{Q}}} & O_{1\times(d_{\mathbb{Q}}-1)}\end{bmatrix}^{\prime}\nonumber \\
G_{XX}^{\mathbb{Q}} & =Jordan(3),\nonumber \\
\epsilon_{X,t}^{\mathbb{Q}} & \backsim\mathcal{N}(O_{d_{\mathbb{Q}}\times1},\Omega_{XX}).\nonumber 
\end{align}

\end_inset

We justify our specific form of 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-eigenvalues in 
\begin_inset Formula $G_{XX}^{\mathbb{Q}}$
\end_inset

 by the below proposition.
 
\end_layout

\begin_layout Proposition
\begin_inset CommandInset label
LatexCommand label
name "prop:JSZ"

\end_inset

When the eigenvalues of 
\begin_inset Formula $G_{XX}^{\mathbb{Q}}-I_{d_{\mathbb{Q}}}$
\end_inset

 is restricted to 
\begin_inset Formula $\{0,\exp(-\kappa^{\mathbb{Q}})-1,\exp(-\kappa^{\mathbb{Q}})-1\}$
\end_inset

,
 the restricted JSZ model with is observationally equivalent to the AFNS model.
\end_layout

\begin_layout Proof
See Appendix 
\begin_inset CommandInset ref
LatexCommand ref
reference "sec:proof_JSZ"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

.
\end_layout

\begin_layout Standard
Setting prior belief about 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

 is easy,
 because 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

 is just the DNS decay parameter.
 It is easy to modify our model to estimate unrestricted 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-eigenvalues if we have reasonable prior belief.
 Reflecting a prior belief on the eigenvalues and estimate unrestricted eigenvalues is straight-forward.
 Considering a great cross-sectional fit of the DNS model,
 the restriction on the 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-eigenvalues is supported empirically.
 
\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{P}$
\end_inset

-dynamics is 
\begin_inset Formula 
\begin{align*}
\underbrace{\begin{bmatrix}\underset{(d_{\mathbb{Q}}\times1)}{X_{t}}\\
M_{t}
\end{bmatrix}}_{F_{t}:d_{\mathbb{P}}\times1} & =\underbrace{\begin{bmatrix}\underset{(d_{\mathbb{Q}}\times1)}{K_{X}^{\mathbb{P}}}\\
K_{M}^{\mathbb{P}}
\end{bmatrix}}_{K_{F}^{\mathbb{P}}:d_{\mathbb{P}}\times1}+\sum_{l=1}^{p}\underbrace{\begin{pmatrix}\underset{(d_{\mathbb{Q}}\times d_{\mathbb{Q}})}{G_{XX,l}^{\mathbb{P}}} & G_{XM,l}^{\mathbb{P}}\\
G_{MX,l}^{\mathbb{P}} & G_{MM,l}^{\mathbb{P}}
\end{pmatrix}}_{G_{FF,l}^{\mathbb{P}}:d_{\mathbb{P}}\times d_{\mathbb{P}}}\begin{bmatrix}X_{t-l}\\
M_{t-l}
\end{bmatrix}+\underbrace{\begin{bmatrix}\underset{(d_{\mathbb{Q}}\times1)}{\epsilon_{X,t}^{\mathbb{P}}}\\
\epsilon_{M,t}^{\mathbb{P}}
\end{bmatrix}}_{\epsilon_{F,t}^{\mathbb{P}}:d_{\mathbb{P}}\times1},\\
\epsilon_{F,t}^{\mathbb{P}} & \backsim\mathcal{N}(O_{d_{\mathbb{P}}\times1},\underbrace{\Omega_{FF}}_{d_{\mathbb{P}}\times d_{\mathbb{P}}}\equiv\begin{pmatrix}\underbrace{\Omega_{XX}}_{d_{\mathbb{Q}}\times d_{\mathbb{Q}}} & \Omega_{MX}^{\prime}\\
\Omega_{MX} & \Omega_{MM}
\end{pmatrix}).
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Bond price
\end_layout

\begin_layout Standard
\begin_inset CommandInset label
LatexCommand label
name "subsec:Bond-price"

\end_inset

Our guess about the bond price formula is 
\begin_inset Formula 
\begin{equation}
P_{\tau,t}=\exp[-a_{\tau}-b_{\tau}^{\prime}X_{t}].\label{eq:pricing}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
We verify our guess with the method of undetermined coefficients.
 Under 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-measure,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
P_{\tau,t}=\mathbb{E}_{t}^{\mathbb{Q}}[\exp[-r_{t}]P_{\tau-1,t+1}].\label{eq:LOP}
\end{equation}

\end_inset

If we combine the above equations,
 
\begin_inset Formula 
\begin{equation}
\exp[-a_{\tau}-b_{\tau}^{\prime}X_{t}]=\mathbb{E}_{t}^{\mathbb{Q}}[\exp[-r_{t}-a_{\tau-1}-b_{\tau-1}^{\prime}X_{t+1}]].\label{eq;aux41}
\end{equation}

\end_inset

By equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:Q_X"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

),
\begin_inset Formula 
\begin{align*}
 & \exp[-a_{\tau}-b_{\tau}^{\prime}X_{t}]\\
= & \mathbb{E}_{t}^{\mathbb{Q}}[\exp[-r_{t}-a_{\tau-1}-b_{\tau-1}^{\prime}(K_{X}^{\mathbb{Q}}+G_{XX}^{\mathbb{Q}}X_{t}+\epsilon_{X,t+1}^{\mathbb{Q}})]],\\
= & \mathbb{E}_{t}^{\mathbb{Q}}[\exp[-r_{t}-a_{\tau-1}-b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}-b_{\tau-1}^{\prime}G_{XX}^{\mathbb{Q}}X_{t}-b_{\tau-1}^{\prime}\epsilon_{X,t+1}^{\mathbb{Q}}]].
\end{align*}

\end_inset

Expectation operator 
\begin_inset Formula $\mathbb{E}_{t}^{\mathbb{Q}}$
\end_inset

 is applied,
 then 
\begin_inset Formula 
\begin{align*}
 & \exp[-a_{\tau}-b_{\tau}^{\prime}X_{t}]\\
= & \exp[-r_{t}-a_{\tau-1}-b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}-b_{\tau-1}^{\prime}G_{XX}^{\mathbb{Q}}X_{t}+0.5b_{\tau-1}^{\prime}\Omega_{XX}b_{\tau-1}].
\end{align*}

\end_inset

We substitute equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:r"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

) into the above expression.
 Then,
\begin_inset Formula 
\begin{align*}
 & \exp[-a_{\tau}-b_{\tau}^{\prime}X_{t}]\\
= & \exp[-a_{\tau-1}-b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}+0.5b_{\tau-1}^{\prime}\Omega_{XX}b_{\tau-1}-(b_{\tau-1}^{\prime}G_{XX}^{\mathbb{Q}}+\iota^{\prime})X_{t}].
\end{align*}

\end_inset

By the method of undetermined coefficient,
 
\begin_inset Formula 
\begin{align*}
\therefore a_{\tau}= & a_{\tau-1}+b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}-0.5b_{\tau-1}^{\prime}\Omega_{XX}b_{\tau-1},\\
b_{\tau}= & \iota+G_{XX}^{\mathbb{Q}\prime}b_{\tau-1}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
In reality,
 the bond yield is determined by
\begin_inset Formula 
\begin{align*}
R_{\tau,t} & =\frac{a_{\tau}}{\tau}+\frac{b_{\tau}^{\prime}}{\tau}X_{t}+e_{\tau,t},\\
e_{\tau,t} & \backsim\mathcal{N}(0,\sigma_{\tau,e}^{2}).
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Affine Transformation to Principal Components
\end_layout

\begin_layout Standard
We use an affine transformation from 
\begin_inset Formula $X_{t}$
\end_inset

 to principal components to estimate our model efficiently.
 For first 
\begin_inset Formula $d_{\mathbb{Q}}$
\end_inset

 principal components 
\begin_inset Formula $\mathcal{P}_{t}$
\end_inset

 and remaining 
\begin_inset Formula $\mathcal{O}_{t}$
\end_inset

,
 
\begin_inset Formula 
\begin{align*}
\begin{bmatrix}\mathcal{P}_{t}-c\\
\mathcal{O}_{t}
\end{bmatrix} & :=\begin{pmatrix}W_{\mathcal{P}}\\
W_{\mathcal{O}}
\end{pmatrix}R_{t}-\begin{bmatrix}c\\
O_{(N-d_{\mathbb{Q}})\times1}
\end{bmatrix}\\
 & =\begin{pmatrix}W_{\mathcal{P}}\mathcal{A}_{X}-c\\
W_{\mathcal{O}}\mathcal{A}_{X}
\end{pmatrix}+\begin{pmatrix}W_{\mathcal{P}}\\
W_{\mathcal{O}}
\end{pmatrix}\mathcal{B}{}_{X}X_{t}+\begin{bmatrix}O_{d_{\mathbb{Q}}\times1}\\
e_{\mathcal{O},t}
\end{bmatrix},\\
\text{where }R_{t}^{\prime} & :=\begin{bmatrix}R_{\tau_{1},t} & R_{\tau_{2},t} & \cdots & R_{\tau_{N},t}\end{bmatrix},\\
\mathcal{A}{}_{X}^{\prime} & :=\begin{bmatrix}a_{\tau_{1}}/\tau_{1} & a_{\tau_{2}}/\tau_{2} & \cdots & a_{\tau_{N}}/\tau_{N}\end{bmatrix},\\
\mathcal{B}_{X}^{\prime} & :=\begin{bmatrix}b_{\tau_{1}}/\tau_{1} & b_{\tau_{2}}/\tau_{2} & \cdots & b_{\tau_{N}}/\tau_{N}\end{bmatrix},\\
e_{\mathcal{O},t} & \backsim\mathcal{N}(O_{(N-d_{\mathbb{Q}})\times1},\Sigma_{\mathcal{O}}:=diag(\begin{bmatrix}\sigma_{O,1}^{2} & \sigma_{O,2}^{2} & \cdots & \sigma_{\mathcal{O},N-d_{\mathbb{Q}}}^{2}\end{bmatrix})),
\end{align*}

\end_inset


\begin_inset Formula $W_{\mathcal{P}}$
\end_inset

 and 
\begin_inset Formula $W_{\mathcal{O}}$
\end_inset

 is rotation matrices for 
\begin_inset Formula $\mathcal{P}_{t}$
\end_inset

 and 
\begin_inset Formula $\mathcal{O}_{t}$
\end_inset

,
 respectively.
 We set 
\begin_inset Formula $c$
\end_inset

 to a sample mean of 
\begin_inset Formula $\mathcal{P}_{t}$
\end_inset

 to make the sample mean of latent pricing factors to zeros.
 Under this assumption,
 
\begin_inset Formula $X_{t}$
\end_inset

 becomes observed factors given parameters,
 because 
\begin_inset Formula 
\begin{align*}
\mathcal{P}_{t}-c & =\mathcal{T}_{0,X}+\mathcal{T}_{1,X}X_{t},\\
\text{where }\mathcal{T}_{0,X}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{XX}) & :=W_{\mathcal{P}}\mathcal{A}_{X}-c,\\
\mathcal{T}_{1,X}(:\kappa^{\mathbb{Q}}) & :=W_{\mathcal{P}}\mathcal{B}{}_{X},
\end{align*}

\end_inset

and
\begin_inset Formula 
\begin{align}
X_{t} & =\mathcal{T}_{0,\mathcal{P}}+\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c),\label{eq:P2X}\\
\text{where }\mathcal{T}_{1,\mathcal{P}}(:\kappa^{\mathbb{Q}}) & :=(W_{\mathcal{P}}\mathcal{B}{}_{X})^{-1}\equiv\mathcal{T}_{1,X}^{-1},\nonumber \\
\mathcal{T}_{0,\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{XX}) & :=-\mathcal{T}_{1,\mathcal{P}}(W_{\mathcal{P}}\mathcal{A}_{X}-c)\equiv-\mathcal{T}_{1,\mathcal{P}}\mathcal{T}_{0,X}.\nonumber 
\end{align}

\end_inset

The parentheses indicate deep-parameters in the latent factor state space.
\end_layout

\begin_layout Standard
Using the transformation
\begin_inset Formula 
\[
\underbrace{\begin{bmatrix}X_{t}\\
M_{t}
\end{bmatrix}}_{F_{t}}=\underbrace{\begin{bmatrix}\mathcal{T}_{0,\mathcal{P}}\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times1}
\end{bmatrix}}_{\mathcal{T}_{0,\mathcal{F}}}+\underbrace{\begin{pmatrix}\mathcal{T}_{1,\mathcal{P}} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times d_{\mathbb{Q}}} & I_{(d_{\mathbb{P}}-d_{\mathbb{Q}})}
\end{pmatrix}}_{\mathcal{T}_{1,\mathcal{F}}}\underbrace{\begin{bmatrix}\mathcal{P}_{t}-c\\
M_{t}
\end{bmatrix}}_{\mathcal{F}_{t}},
\]

\end_inset

we rotate our JSZ model.
 The rotated short-term interest rate is
\begin_inset Formula 
\begin{equation}
r_{t}=\underbrace{\iota^{\prime}\mathcal{T}_{0,\mathcal{P}}}_{\delta(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{XX})}+\underbrace{\iota^{\prime}\mathcal{T}_{1,\mathcal{P}}}_{\beta(:\kappa^{\mathbb{Q}})^{\prime}}(\mathcal{P}_{t}-c),\label{eq:r_P}
\end{equation}

\end_inset

and the rotated 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics is
\begin_inset Formula 
\begin{align*}
\mathcal{P}_{t}-c & =\underbrace{\mathcal{T}_{1,X}[K_{X}^{\mathbb{Q}}+(G_{XX}^{\mathbb{Q}}-I_{d_{\mathbb{Q}}})\mathcal{T}_{0,\mathcal{P}}]}_{K_{\mathcal{P}}^{\mathbb{Q}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{XX})}+\underbrace{\mathcal{T}_{1,X}G_{XX}^{\mathbb{Q}}\mathcal{T}_{1,\mathcal{P}}}_{G_{\mathcal{PP}}^{\mathbb{Q}}(:\kappa^{\mathbb{Q}})}(\mathcal{P}_{t-1}-c)+\underbrace{\mathcal{T}_{1,X}\epsilon_{X,t}^{\mathbb{Q}}}_{\epsilon_{\mathcal{P},t}^{\mathbb{Q}}},\\
\epsilon_{\mathcal{P},t}^{\mathbb{Q}} & \backsim\mathcal{N}(O_{d_{\mathbb{Q}}\times1},\Omega_{\mathcal{PP}}\equiv\mathcal{T}_{1,X}\Omega_{XX}\mathcal{T}_{1,X}^{\prime}).
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
State-Space Representation
\end_layout

\begin_layout Standard
The rotated model has a measurement equation of
\begin_inset Formula 
\[
\mathcal{O}_{t}=\underbrace{W_{\mathcal{O}}[\mathcal{A}_{X}+\mathcal{B}{}_{X}\mathcal{T}_{0,\mathcal{P}}]}_{\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{XX})}+\underbrace{W_{\mathcal{O}}\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}}_{\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})}(\mathcal{P}_{t}-c)+e_{\mathcal{O},t}.
\]

\end_inset

The 
\begin_inset Formula $\mathbb{P}$
\end_inset

-transition equation is
\begin_inset Formula 
\begin{align*}
\mathcal{F}_{t} & =\underbrace{\begin{bmatrix}\mathcal{T}_{1,X}[K_{X}^{\mathbb{P}}-(I_{d_{\mathbb{Q}}}-\sum_{l=1}^{p}G_{XX,l}^{\mathbb{P}})\mathcal{T}_{0,\mathcal{P}}]\\
K_{M}^{\mathbb{P}}+(\sum_{l=1}^{p}G_{MX,l}^{\mathbb{P}})\mathcal{T}_{0,\mathcal{P}}
\end{bmatrix}}_{K_{\mathcal{F}}^{\mathbb{P}}}\\
 & +\sum_{l=1}^{p}\underbrace{\begin{pmatrix}\mathcal{T}_{1,X}G_{XX,l}^{\mathbb{P}}\mathcal{T}_{1,\mathcal{P}} & \mathcal{T}_{1,X}G_{XM,l}^{\mathbb{P}}\\
G_{MX,l}^{\mathbb{P}}\mathcal{T}_{1,\mathcal{P}} & G_{MM,l}^{\mathbb{P}}
\end{pmatrix}}_{G_{\mathcal{FF},l}^{\mathbb{P}}}\mathcal{F}_{t-l}+\underbrace{\begin{bmatrix}\mathcal{T}_{1,X}\epsilon_{X,t}^{\mathbb{P}}\\
\epsilon_{M,t}^{\mathbb{P}}
\end{bmatrix}}_{\epsilon_{\mathcal{F},t}^{\mathbb{P}}}.
\end{align*}

\end_inset

It is easy to verify that there is a sufficient degree of freedom to set parameters,
 
\begin_inset Formula $K_{\mathcal{F}}^{\mathbb{P}}$
\end_inset

,
 
\begin_inset Formula $G_{\mathcal{FF},l}^{\mathbb{P}}$
\end_inset

 and the covariance of 
\begin_inset Formula $\epsilon_{\mathcal{F},t}^{\mathbb{P}}$
\end_inset

,
 as unrestricted parameters.
 Therefore,
 we can estimate the 
\begin_inset Formula $\mathbb{P}$
\end_inset

-dynamics with a unrestricted 
\begin_inset Formula $VAR(p)$
\end_inset

 model of 
\begin_inset Formula 
\begin{align}
\mathcal{F}_{t} & =\underbrace{\begin{bmatrix}K_{\mathcal{P}}^{\mathbb{P}}\\
K_{M|\mathcal{F}}^{\mathbb{P}}
\end{bmatrix}}_{K_{\mathcal{F}}^{\mathbb{P}}}+\sum_{l=1}^{p}\underbrace{\begin{pmatrix}G_{\mathcal{PP},l}^{\mathbb{P}} & G_{\mathcal{P}M,l}^{\mathbb{P}}\\
G_{M\mathcal{P},l}^{\mathbb{P}} & G_{MM,l}^{\mathbb{P}}
\end{pmatrix}}_{G_{\mathcal{FF},l}^{\mathbb{P}}}\mathcal{F}_{t-l}+\underbrace{\begin{bmatrix}\epsilon_{\mathcal{P},t}^{\mathbb{P}}\\
\epsilon_{M,t}^{\mathbb{P}}
\end{bmatrix}}_{\epsilon_{\mathcal{F},t}^{\mathbb{P}}},\label{eq:P}\\
\epsilon_{\mathcal{F},t}^{\mathbb{P}} & \backsim\mathcal{N}(O_{d_{\mathbb{P}}\times1},\Omega_{\mathcal{FF}}:=\begin{pmatrix}\Omega_{\mathcal{PP}} & \Omega_{M\mathcal{P}}^{\prime}\\
\Omega_{M\mathcal{P}} & \Omega_{MM}
\end{pmatrix}:=\begin{pmatrix}\mathcal{T}_{1,X}\Omega_{XX}\mathcal{T}_{1,X}^{\prime} & \mathcal{T}_{1,X}\Omega_{MX}^{\prime}\\
\Omega_{MX}\mathcal{T}_{1,X}^{\prime} & \Omega_{MM}
\end{pmatrix}).\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Given 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

,
 there is a one-to-one mapping between 
\begin_inset Formula $\Omega_{\mathcal{PP}}$
\end_inset

 and 
\begin_inset Formula $\Omega_{XX}$
\end_inset

.
 Therefore,
 we can write the state-space equations as
\begin_inset Formula 
\begin{align}
\mathcal{O}_{t} & =\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t}-c)+e_{\mathcal{O},t},\label{eq:mea}\\
\mathcal{F}_{t} & =K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t-l}+\epsilon_{\mathcal{F},t}^{\mathbb{P}}.\label{eq:trans}
\end{align}

\end_inset

In this case,
 the difference equations are
\begin_inset Formula 
\begin{align*}
a_{\tau} & =a_{\tau-1}+b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}-0.5b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1},\\
b_{\tau} & =\iota+G_{XX}^{\mathbb{Q}\prime}b_{\tau-1},
\end{align*}

\end_inset

and also the short rate and 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics are
\begin_inset Formula 
\begin{align}
r_{t} & =\delta(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\beta(:\kappa^{\mathbb{Q}})^{\prime}(\mathcal{P}_{t}-c),\nonumber \\
\mathcal{P}_{t} & -c=K_{\mathcal{P}}^{\mathbb{Q}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+G_{\mathcal{PP}}^{\mathbb{Q}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t-1}-c)+\epsilon_{\mathcal{P},t}^{\mathbb{Q}}.\label{eq:Q}
\end{align}

\end_inset


\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics can be re-written with macro variables,
 for example,
 
\begin_inset Formula 
\begin{align}
\mathcal{F}_{t} & =\underbrace{\begin{bmatrix}K_{\mathcal{P}}^{\mathbb{Q}}\\
K_{M|\mathcal{F}}^{\mathbb{P}}
\end{bmatrix}}_{K_{\mathcal{F}}^{\mathbb{Q}}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{Q}}\mathcal{F}_{t-l}+\underbrace{\begin{bmatrix}\epsilon_{\mathcal{P},t}^{\mathbb{Q}}\\
\epsilon_{M,t}^{\mathbb{P}}
\end{bmatrix}}_{\epsilon_{\mathcal{F},t}^{\mathbb{Q}}},\label{eq:Q_macro}\\
G_{\mathcal{FF},l}^{\mathbb{Q}} & =\begin{cases}
\begin{pmatrix}G_{\mathcal{PP}}^{\mathbb{Q}} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\\
G_{M\mathcal{P},1}^{\mathbb{P}} & G_{MM,1}^{\mathbb{P}}
\end{pmatrix} & \text{if }l=1,\\
\begin{pmatrix}O_{d_{\mathbb{Q}}\times d_{\mathbb{Q}}} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\\
G_{M\mathcal{P},l}^{\mathbb{P}} & G_{MM,l}^{\mathbb{P}}
\end{pmatrix} & \text{otherwise.}
\end{cases}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Subsection
Moving to deep-parameters
\end_layout

\begin_layout Standard
Given 
\begin_inset Formula $\{\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},K_{\mathcal{F}}^{\mathbb{P}},G_{\mathcal{FF},l}^{\mathbb{P}},\Omega_{\mathcal{FF}}\}$
\end_inset

,
 we can get the deep-parameters by
\begin_inset Formula 
\begin{align}
\Omega_{XX} & =\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime},\label{eq:OmegaPP2OmegaXX}\\
\Omega_{MX} & =\Omega_{M\mathcal{P}}\mathcal{T}_{1,\mathcal{P}}^{\prime},\nonumber \\
G_{XX,l}^{\mathbb{P}} & =\mathcal{T}_{1,\mathcal{P}}G_{\mathcal{PP},l}^{\mathbb{P}}\mathcal{T}_{1,X},\nonumber \\
G_{MX,l}^{\mathbb{P}} & =G_{M\mathcal{P},l}^{\mathbb{P}}\mathcal{T}_{1,X},\nonumber \\
G_{XM,l}^{\mathbb{P}} & =\mathcal{T}_{1,\mathcal{P}}G_{\mathcal{P}M,l}^{\mathbb{P}},\nonumber \\
K_{X}^{\mathbb{P}} & =\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{P}}+(I_{d_{\mathbb{Q}}}-\sum_{l=1}^{p}G_{XX,l}^{\mathbb{P}})\mathcal{T}_{0,\mathcal{P}},\nonumber \\
K_{M}^{\mathbb{P}} & =K_{M|\mathcal{F}}^{\mathbb{P}}-(\sum_{l=1}^{p}G_{MX,l}^{\mathbb{P}})\mathcal{T}_{0,\mathcal{P}},\nonumber \\
G_{XX}^{\mathbb{Q}} & =\mathcal{T}_{1,\mathcal{P}}G_{\mathcal{PP}}^{\mathbb{Q}}\mathcal{T}_{1,X},\label{eq:GQPP2GQXX}\\
K_{X}^{\mathbb{Q}} & =\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{Q}}+(I_{d_{\mathbb{Q}}}-G_{XX}^{\mathbb{Q}})\mathcal{T}_{0,\mathcal{P}}.\label{eq:KQP2KQX}
\end{align}

\end_inset


\end_layout

\begin_layout Subsection
Incorporation of the fourth observable spanned factor
\end_layout

\begin_layout Standard
Suppose the fourth factor is observable by a vector 
\begin_inset Formula $Z_{t}$
\end_inset

,
 so the pricing factor 
\begin_inset Formula $X_{t}$
\end_inset

 is 
\begin_inset Formula 
\[
X_{t}:=\begin{bmatrix}X_{t,(1:3)}\\
Z_{t}
\end{bmatrix}.
\]

\end_inset

 In this case,
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\begin{bmatrix}\mathcal{P}_{t}-c\\
Z_{t}\\
\mathcal{O}_{t}
\end{bmatrix} & =\begin{pmatrix}W_{\mathcal{P}}\\
O_{d_{z}\times1}\\
W_{\mathcal{O}}
\end{pmatrix}R_{t}-\begin{bmatrix}c\\
-Z_{t}\\
O_{(N-d_{\mathbb{Q}})\times1}
\end{bmatrix},\\
 & =\begin{pmatrix}W_{\mathcal{P}}\mathcal{A}_{X}-c\\
O_{d_{z}\times1}\\
W_{\mathcal{O}}\mathcal{A}_{X}
\end{pmatrix}+\begin{pmatrix} & W_{\mathcal{P}}\mathcal{B}{}_{X}\\
O_{d_{z}\times3} & I_{d_{z}}\\
 & W_{\mathcal{O}}\mathcal{B}{}_{X}
\end{pmatrix}X_{t}+\begin{bmatrix}O_{d_{\mathbb{Q}}\times1}\\
O_{d_{z}\times1}\\
e_{\mathcal{O},t}
\end{bmatrix}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The inverse of the transformation would be 
\begin_inset Formula 
\begin{align*}
\begin{bmatrix}\mathcal{P}_{t}-c\\
Z_{t}
\end{bmatrix} & =\begin{bmatrix}\mathcal{T}_{0,X}\\
O_{d_{z}\times1}
\end{bmatrix}+\begin{pmatrix} & \mathcal{T}_{1,X}\\
O_{d_{z}\times3} & I_{d_{z}}
\end{pmatrix}X_{t},
\end{align*}

\end_inset

and the transformation is 
\begin_inset Formula 
\begin{align*}
X_{t}=-\begin{pmatrix} & \mathcal{T}_{1,X}\\
O_{d_{z}\times3} & I_{d_{z}}
\end{pmatrix}^{-1}\begin{bmatrix}\mathcal{T}_{0,X}\\
O_{d_{z}\times1}
\end{bmatrix}+\begin{pmatrix} & \mathcal{T}_{1,X}\\
O_{d_{z}\times3} & I_{d_{z}}
\end{pmatrix}^{-1}\begin{bmatrix}\mathcal{P}_{t}-c\\
Z_{t}
\end{bmatrix}.
\end{align*}

\end_inset

Therefore,
 the coefficients of the transformation can be extended by augmenting the identity matrix and zeros.
\end_layout

\begin_layout Section
Stochastic Discount Factor
\end_layout

\begin_layout Standard
Our market prices of risks are 
\begin_inset Formula 
\begin{align}
\Omega_{\mathcal{FF}}^{1/2}\underset{(d_{\mathbb{P}}\times1)}{\lambda_{t}} & \equiv\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{F}_{t+1}]-\mathbb{E}_{t}^{\mathbb{Q}}[\mathcal{F}_{t+1}],\label{eq:aux4}\\
 & =\underbrace{K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}}}_{\lambda:d_{\mathbb{P}}\times1}+\sum_{l=1}^{p}(\underbrace{G_{\mathcal{FF},l}^{\mathbb{P}}-G_{\mathcal{FF},l}^{\mathbb{Q}}}_{\Lambda_{l}:d_{\mathbb{P}}\times d_{\mathbb{P}}})\mathcal{F}_{t+1-l},\label{eq:mpr}\\
 & =\begin{bmatrix}\lambda_{\mathcal{P}}+\sum_{l=1}^{p}\Lambda_{\mathcal{PP},l}\mathcal{P}_{t+1-l}+\sum_{l=1}^{p}\Lambda_{\mathcal{P}M,l}M_{t+1-l}\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times1}
\end{bmatrix}.\label{eq:frp}\\
\text{where }\lambda & =\begin{bmatrix}\lambda_{\mathcal{P}}\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times1}
\end{bmatrix},\label{eq:lambda}\\
\underset{(d_{\mathbb{Q}}\times1)}{\lambda_{\mathcal{P}}} & =K_{\mathcal{P}}^{\mathbb{P}}-K_{\mathcal{P}}^{\mathbb{Q}},\label{eq:lambda_P}\\
\Lambda_{l} & =\begin{pmatrix}\Lambda_{\mathcal{PP},l} & \Lambda_{\mathcal{P}M,l}\\
O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times d_{\mathbb{Q}}} & O_{(d_{\mathbb{P}}-d_{\mathbb{Q}})\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}
\end{pmatrix},\label{eq:Lambda}\\
\underset{(d_{\mathbb{Q}}\times d_{\mathbb{Q}})}{\Lambda_{\mathcal{PP},l}} & =\begin{cases}
G_{\mathcal{PP},1}^{\mathbb{P}}-G_{\mathcal{PP}}^{\mathbb{Q}} & \text{if }l=1,\\
G_{\mathcal{PP},l}^{\mathbb{P}} & \text{otherwise,}
\end{cases}\label{eq:Lambda_PP}\\
\underset{(d_{\mathbb{Q}}\times(d_{\mathbb{P}}-d_{\mathbb{Q}}))}{\Lambda_{\mathcal{P}M,l}} & =G_{\mathcal{P}M,l}^{\mathbb{P}}.\label{eq:Lambda_PM}
\end{align}

\end_inset


\begin_inset CommandInset citation
LatexCommand citep
key "CheriditoFK07"
literal "false"

\end_inset

.
 
\end_layout

\begin_layout Standard
Next,
 we derive the Radon-Nikodym derivative 
\begin_inset Formula $d\mathbb{Q}/d\mathbb{P}$
\end_inset

 to specify a relationship between the two measures.
 By equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:P"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

),
 risk factor 
\begin_inset Formula $\mathcal{F}_{t}$
\end_inset

 has 
\begin_inset Formula 
\begin{equation}
\mathcal{F}_{t+1}=K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t+1-l}+\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}\label{eq;dynP}
\end{equation}

\end_inset

in the 
\begin_inset Formula $\mathbb{P}$
\end_inset

-measure.
 We transform the above 
\begin_inset Formula $\mathbb{P}$
\end_inset

-dynamics into 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics by making 
\begin_inset Formula $\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}-(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l})$
\end_inset

 in the above equation.
 So,
 
\begin_inset Formula 
\begin{align*}
\mathcal{F}_{t+1} & =K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t+1-l}+\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}-(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}),\\
 & =(K_{\mathcal{F}}^{\mathbb{P}}-\lambda)+\sum_{l=1}^{p}(G_{\mathcal{FF},l}^{\mathbb{P}}-\Lambda_{l})\mathcal{F}_{t+1-l}+\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}.
\end{align*}

\end_inset

By equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:mpr"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
 the above equation can be written as
\begin_inset Formula 
\[
\mathcal{F}_{t+1}=K_{\mathcal{F}}^{\mathbb{Q}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{Q}}\mathcal{F}_{t+1-l}+\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}.
\]

\end_inset

By comparing the above expression with equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:Q_macro"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

),
 we derive the relationship between 
\begin_inset Formula $\epsilon_{t+1}^{\mathbb{P}}$
\end_inset

 and 
\begin_inset Formula $\epsilon_{t+1}^{\mathbb{Q}}.$
\end_inset


\begin_inset Formula 
\begin{align*}
\epsilon_{\mathcal{F},t+1}^{\mathbb{Q}} & =\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l},\\
\epsilon_{\mathcal{F},t+1}^{\mathbb{P}} & =\epsilon_{\mathcal{F},t+1}^{\mathbb{Q}}-(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}).
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Under 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-measure,
 since 
\begin_inset Formula $\epsilon_{\mathcal{F},t+1}^{\mathbb{Q}}$
\end_inset

 follows 
\begin_inset Formula $\mathcal{N}(O_{d_{\mathbb{P}}\times1},\Omega_{\mathcal{FF}})$
\end_inset

,
\begin_inset Formula 
\begin{align*}
p^{\mathbb{Q}}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|\mathcal{I}_{t}) & =\mathcal{N}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|-(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}),\Omega_{\mathcal{FF}})\text{ under }\mathbb{Q}\text{-measure.}
\end{align*}

\end_inset

On the other hand,
 
\begin_inset Formula 
\begin{align*}
p^{\mathbb{P}}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|\mathcal{I}_{t}) & =\mathcal{N}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|O_{d_{\mathbb{P}}\times1},\Omega_{\mathcal{FF}})\text{ under the }\mathbb{P}\text{-measure.}
\end{align*}

\end_inset

Radon-Nikodym derivative 
\begin_inset Formula $d\mathbb{P}/d\mathbb{Q}$
\end_inset

 is the one that satisfies
\begin_inset Formula 
\[
p^{\mathbb{Q}}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|\mathcal{I}_{t})=\frac{d\mathbb{Q}}{d\mathbb{P}}p^{\mathbb{P}}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|\mathcal{I}_{t}).
\]

\end_inset

If we substitute the above derivations into the above equation,
\begin_inset Formula 
\[
\mathcal{N}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|-(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}),\Omega_{\mathcal{FF}})=\frac{d\mathbb{Q}}{d\mathbb{P}}\mathcal{N}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}|O_{d_{\mathbb{P}}\times1},\Omega_{\mathcal{FF}}).
\]

\end_inset

By the functional form of a normal density,
 the above equation is 
\begin_inset Formula 
\begin{align*}
 & (2\pi)^{-\frac{d_{\mathbb{P}}}{2}}\det\left(\Omega_{\mathcal{FF}}\right)^{-\frac{1}{2}}\exp[-\frac{1}{2}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}))^{\prime}\Omega_{\mathcal{FF}}^{-1}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}))]\\
= & \frac{d\mathbb{Q}}{d\mathbb{P}}(2\pi)^{-\frac{d_{\mathbb{P}}}{2}}\det\left(\Omega_{\mathcal{FF}}\right)^{-\frac{1}{2}}\exp[-\frac{1}{2}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}\prime}\Omega_{\mathcal{FF}}^{-1}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}],
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d\mathbb{Q}}{d\mathbb{P}}\exp[-\frac{1}{2}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}\prime}\Omega_{\mathcal{FF}}^{-1}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}] & =\exp[-\frac{1}{2}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}))^{\prime}\Omega_{\mathcal{FF}}^{-1}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+(\lambda+\sum_{l=1}^{p}\Lambda_{l}\mathcal{F}_{t+1-l}))].
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
By equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:mpr"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

),
 the right-hand side can be simplified as
\begin_inset Formula 
\[
\exp[-\frac{1}{2}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\Omega_{\mathcal{FF}}^{1/2}\lambda_{t})^{\prime}\Omega_{\mathcal{FF}}^{-1}(\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\Omega_{\mathcal{FF}}^{1/2}\lambda_{t})].
\]

\end_inset

We expand the above equation as
\begin_inset Formula 
\begin{align*}
 & \exp[-\frac{1}{2}\{\epsilon_{\mathcal{F},t+1}^{\mathbb{P}\prime}\Omega_{\mathcal{FF}}^{-1}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+2\lambda_{t}^{\prime}\Omega_{\mathcal{FF}}^{-1/2}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}+\lambda_{t}^{\prime}\lambda_{t}\}]\\
= & \exp[-\frac{1}{2}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}\prime}\Omega_{\mathcal{FF}}^{-1}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}].
\end{align*}

\end_inset

Therefore,
 if we substitute the last expression into the original equation,
\begin_inset Formula 
\begin{align*}
\frac{d\mathbb{Q}}{d\mathbb{P}}\exp[-\frac{1}{2}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}\prime}\Omega_{\mathcal{FF}}^{-1}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}] & =\exp[-\frac{1}{2}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}\prime}\Omega_{\mathcal{FF}}^{-1}\epsilon_{\mathcal{F},t+1}^{\mathbb{P}}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}].
\end{align*}

\end_inset

We simplify the above equation with respect to 
\begin_inset Formula $d\mathbb{Q}/d\mathbb{P}$
\end_inset

,
 and it leads to 
\begin_inset Formula 
\[
\frac{d\mathbb{Q}}{d\mathbb{P}}=\exp[-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}].
\]

\end_inset

Therefore,
 the model-consistent SDF 
\begin_inset Formula $\mathcal{M}_{t+1}^{\mathbb{P}}$
\end_inset

 should be
\begin_inset Formula 
\begin{align}
\mathcal{M}_{t+1}^{\mathbb{P}} & \equiv\exp[-r_{t}]\frac{d\mathbb{Q}}{d\mathbb{P}},\nonumber \\
 & =\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}].\label{eq;sdfp}
\end{align}

\end_inset


\end_layout

\begin_layout Subsection
Bond price under 
\begin_inset Formula $\mathbb{P}$
\end_inset

 measure
\end_layout

\begin_layout Standard
By equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq;sdfp"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
P_{\tau,t} & =\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{M}_{t+1}^{\mathbb{P}}P_{\tau-1,t+1}],\\
 & =\mathbb{E}_{t}^{\mathbb{P}}[\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}]P_{\tau-1,t+1}].
\end{align*}

\end_inset

Also,
 by equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:pricing"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
\begin_inset Formula 
\begin{align*}
 & \exp[-a_{\tau}-b_{\tau}^{\prime}X_{t}]\\
= & \mathbb{E}_{t}^{\mathbb{P}}[\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}-a_{\tau-1}-b_{\tau-1}^{\prime}X_{t+1}]],\\
1= & \mathbb{E}_{t}^{\mathbb{P}}[\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}-a_{\tau-1}-b_{\tau-1}^{\prime}X_{t+1}+a_{\tau}+b_{\tau}^{\prime}X_{t}]],\\
0= & \log\mathbb{E}_{t}^{\mathbb{P}}[\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}-a_{\tau-1}-b_{\tau-1}^{\prime}X_{t+1}+a_{\tau}+b_{\tau}^{\prime}X_{t}]],\\
0= & \log\mathbb{E}_{t}^{\mathbb{P}}[\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}-a_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{0,\mathcal{P}}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t+1}-c)\\
 & +a_{\tau}+b_{\tau}^{\prime}\mathcal{T}_{0,\mathcal{P}}+b_{\tau}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)]],\\
0= & \log\mathbb{E}_{t}^{\mathbb{P}}[\exp[-r_{t}-\frac{1}{2}\lambda_{t}^{\prime}\lambda_{t}-\lambda_{t}^{\prime}v_{\mathcal{F},t+1}^{\mathbb{P}}-a_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{0,\mathcal{P}}+a_{\tau}+b_{\tau}^{\prime}\mathcal{T}_{0,\mathcal{P}}+b_{\tau}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)\\
 & -b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\{K_{\mathcal{P}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{PP},l}^{\mathbb{P}}(\mathcal{P}_{t+1-l}-c)+\sum_{l=1}^{p}G_{\mathcal{P}M,l}^{\mathbb{P}}M_{t+1-l}+\epsilon_{\mathcal{P},t+1}^{\mathbb{P}}\}]].
\end{align*}

\end_inset

We focus on the RHS.
 By equations 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:v"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:r_P"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
 the RHS is
\begin_inset Formula 
\begin{align*}
 & \log\mathbb{E}_{t}^{\mathbb{P}}[\exp[-\iota^{\prime}\mathcal{T}_{0,\mathcal{P}}-\iota^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)-\frac{1}{2}\lambda_{t,(1:d_{\mathbb{Q}})}^{\prime}\lambda_{t,(1:d_{\mathbb{Q}})}-\frac{1}{2}\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}^{\prime}\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}\\
 & -\lambda_{t,(1:d_{\mathbb{Q}})}^{\prime}v_{\mathcal{P},t+1}^{\mathbb{P}}-\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}^{\prime}v_{M,t+1}^{\mathbb{P}}-a_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{0,\mathcal{P}}+a_{\tau}+b_{\tau}^{\prime}\mathcal{T}_{0,\mathcal{P}}+b_{\tau}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)\\
 & -b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{P}}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{PP},l}^{\mathbb{P}}(\mathcal{P}_{t+1-l}-c)-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{P}M,l}^{\mathbb{P}}M_{t+1-l}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}^{1/2}v_{\mathcal{P},t+1}^{\mathbb{P}})]],\\
= & -\iota^{\prime}\mathcal{T}_{0,\mathcal{P}}-\iota^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)-\frac{1}{2}\lambda_{t,(1:d_{\mathbb{Q}})}^{\prime}\lambda_{t,(1:d_{\mathbb{Q}})}-\frac{1}{2}\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}^{\prime}\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}\\
 & -a_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{0,\mathcal{P}}+a_{\tau}+b_{\tau}^{\prime}\mathcal{T}_{0,\mathcal{P}}+b_{\tau}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)\\
 & -b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{P}}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{PP},l}^{\mathbb{P}}(\mathcal{P}_{t+1-l}-c)-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{P}M,l}^{\mathbb{P}}M_{t+1-l}\\
 & +\frac{1}{2}\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}^{\prime}\lambda_{t,(d_{\mathbb{Q}}+1:d_{\mathbb{P}})}+\frac{1}{2}(\lambda_{t,(1:d_{\mathbb{Q}})}^{\prime}+b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}^{1/2})(\lambda_{t,(1:d_{\mathbb{Q}})}^{\prime}+b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}^{1/2})^{\prime},\\
= & -\iota^{\prime}\mathcal{T}_{0,\mathcal{P}}-\iota^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)-a_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{0,\mathcal{P}}+a_{\tau}+b_{\tau}^{\prime}\mathcal{T}_{0,\mathcal{P}}+b_{\tau}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)\\
 & -b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{P}}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{PP},l}^{\mathbb{P}}(\mathcal{P}_{t+1-l}-c)-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{P}M,l}^{\mathbb{P}}M_{t+1-l}\\
 & +b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}^{1/2}\lambda_{t,(1:d_{\mathbb{Q}})}+\frac{1}{2}b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1}.
\end{align*}

\end_inset

By equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:frp"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
 the RHS is
\begin_inset Formula 
\begin{align*}
 & -\iota^{\prime}\mathcal{T}_{0,\mathcal{P}}-\iota^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)-a_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{0,\mathcal{P}}+a_{\tau}+b_{\tau}^{\prime}\mathcal{T}_{0,\mathcal{P}}+b_{\tau}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathcal{P}_{t}-c)\\
 & -b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{P}}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{PP},l}^{\mathbb{P}}(\mathcal{P}_{t+1-l}-c)-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\sum_{l=1}^{p}G_{\mathcal{P}M,l}^{\mathbb{P}}M_{t+1-l}+\frac{1}{2}b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1}\\
 & +b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}(K_{\mathcal{P}}^{\mathbb{P}}-K_{\mathcal{P}}^{\mathbb{Q}}+(G_{\mathcal{PP},1}^{\mathbb{P}}-G_{\mathcal{PP}}^{\mathbb{Q}})(\mathcal{P}_{t}-c)+\sum_{l=2}^{p}G_{\mathcal{PP},l}^{\mathbb{P}}(\mathcal{P}_{t+1-l}-c)+\sum_{l=1}^{p}G_{\mathcal{P}M,l}^{\mathbb{P}}M_{t+1-l}),\\
= & -a_{\tau-1}+a_{\tau}+(b_{\tau}^{\prime}-\iota^{\prime}-b_{\tau-1}^{\prime})\mathcal{T}_{0,\mathcal{P}}+\frac{1}{2}b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{Q}}\\
 & +[(b_{\tau}^{\prime}-\iota^{\prime})\mathcal{T}_{1,\mathcal{P}}-b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}G_{\mathcal{PP}}^{\mathbb{Q}}](\mathcal{P}_{t}-c).
\end{align*}

\end_inset

Therefore,
\begin_inset Formula 
\begin{align*}
a_{\tau} & =a_{\tau-1}-(b_{\tau}^{\prime}-\iota^{\prime}-b_{\tau-1}^{\prime})\mathcal{T}_{0,\mathcal{P}}+b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}K_{\mathcal{P}}^{\mathbb{Q}}-\frac{1}{2}b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1},\\
b_{\tau} & =\iota+(\mathcal{T}_{1,\mathcal{P}}G_{\mathcal{PP}}^{\mathbb{Q}}\mathcal{T}_{1,X})^{\prime}b_{\tau-1}.
\end{align*}

\end_inset

In section 
