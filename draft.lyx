TP_{\tau,t} & =\frac{1}{\tau}\sum_{i=1}^{\tau-1}\mathbb{E}_{t}^{\mathbb{P}}exr_{\tau-i+1,t+i-1},\\
 & =\frac{1}{\tau}\sum_{i=1}^{\tau-1}\mathbb{E}_{t}^{\mathbb{P}}[-0.5b_{\tau-i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-i}-\begin{bmatrix}b_{\tau-i}^{\prime} & O_{1\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\end{bmatrix}\mathcal{T}_{1,\mathcal{F}}\Omega_{\mathcal{FF}}^{1/2}\lambda_{t+i-1}],\\
 & =-\frac{1}{\tau}\sum_{i=1}^{\tau-1}0.5b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{i}-\frac{1}{\tau}\sum_{i=1}^{\tau-1}\begin{bmatrix}b_{\tau-i}^{\prime} & O_{1\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\end{bmatrix}\mathcal{T}_{1,\mathcal{F}}\mathbb{E}_{t}^{\mathbb{P}}[\Omega_{\mathcal{FF}}^{1/2}\lambda_{t+i-1}].
\end{align*}

\end_inset

and,
 by equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:mpr"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
\begin_inset Formula 
\begin{align*}
TP_{\tau,t}= & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}(0.5b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{i}+\begin{bmatrix}b_{\tau-i}^{\prime} & O_{1\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\end{bmatrix}\mathcal{T}_{1,\mathcal{F}}\lambda)\\
 & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}\begin{bmatrix}b_{\tau-i}^{\prime} & O_{1\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\end{bmatrix}\mathcal{T}_{1,\mathcal{F}}(\sum_{l=1}^{p}\Lambda_{l}\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{F}_{t+i-l}]).
\end{align*}

\end_inset

We calculate 
\begin_inset Formula $\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{F}_{t+i-l}]$
\end_inset

 iteratively using 
\begin_inset Formula $\mathbb{P}$
\end_inset

-dynamics 
\begin_inset Formula $\mathcal{F}_{t}=K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t-l}+\epsilon_{\mathcal{F},t}^{\mathbb{P}}$
\end_inset

.
 
\end_layout

\begin_layout Standard
Or,
 by using equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:exr2"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
\begin_inset Formula 
\begin{align*}
TP_{\tau,t}= & \frac{1}{\tau}\sum_{i=1}^{\tau-1}\mathbb{E}_{t}^{\mathbb{P}}exr_{\tau-i+1,t+i-1},\\
= & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}(0.5b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{i}+b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\lambda_{\mathcal{P}})\\
 & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}b_{\tau-i}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\sum_{l=1}^{p}\Lambda_{\mathcal{PP},l}\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{P}_{t+i-l}-c]+\sum_{l=1}^{p}\Lambda_{\mathcal{P}M,l}\mathbb{E}_{t}^{\mathbb{P}}[M_{t+i-l}]).
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Empirical Methodology
\end_layout

\begin_layout Standard
We estimate the model in terms of annual returns.
 Originally,
 the measurement equation is
\begin_inset Formula 
\begin{align*}
\mathcal{O}_{t} & =W_{\mathcal{O}}R_{t}=\mathcal{A}_{\mathcal{P}}+\mathcal{B}_{\mathcal{P}}(\mathcal{P}_{t}-c)+\mathcal{N}(O_{(N-d_{\mathbb{Q}})\times1},\Sigma_{\mathcal{O}}),\\
\mathcal{A}_{\mathcal{P}} & =W_{\mathcal{O}}[\mathcal{A}_{X}+\mathcal{B}{}_{X}\mathcal{T}_{0,\mathcal{P}}],\\
\mathcal{B}_{\mathcal{P}} & =W_{\mathcal{O}}\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}},\\
\mathcal{T}_{1,\mathcal{P}} & =(W_{\mathcal{P}}\mathcal{B}{}_{X})^{-1},\\
\mathcal{T}_{0,\mathcal{P}} & =-\mathcal{T}_{1,\mathcal{P}}(W_{\mathcal{P}}\mathcal{A}_{X}-c)=-\mathcal{T}_{1,\mathcal{P}}\mathcal{T}_{0,X},\\
\mathcal{A}{}_{X}^{\prime} & =\begin{bmatrix}a_{\tau_{1}}/\tau_{1} & a_{\tau_{2}}/\tau_{2} & \cdots & a_{\tau_{N}}/\tau_{N}\end{bmatrix},\\
\mathcal{B}_{X}^{\prime} & =\begin{bmatrix}b_{\tau_{1}}/\tau_{1} & b_{\tau_{2}}/\tau_{2} & \cdots & b_{\tau_{N}}/\tau_{N}\end{bmatrix},\\
a_{\tau} & =a_{\tau-1}+b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}-0.5b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1},\\
b_{\tau} & =\iota+G_{XX}^{\mathbb{Q}\prime}b_{\tau-1},
\end{align*}

\end_inset

and the transition equation is 
\begin_inset Formula 
\begin{align*}
\begin{bmatrix}\mathcal{P}_{t}-c\\
M_{t}
\end{bmatrix} & =\begin{bmatrix}K_{\mathcal{P}}^{\mathbb{P}}\\
K_{M|\mathcal{F}}^{\mathbb{P}}
\end{bmatrix}+\sum_{l=1}^{p}\begin{pmatrix}G_{\mathcal{PP},l}^{\mathbb{P}} & G_{\mathcal{P}M,l}^{\mathbb{P}}\\
G_{M\mathcal{P},l}^{\mathbb{P}} & G_{MM,l}^{\mathbb{P}}
\end{pmatrix}\begin{bmatrix}\mathcal{P}_{t-l}-c\\
M_{t-l}
\end{bmatrix}+\mathcal{N}(O_{d_{\mathbb{P}}\times1},\begin{pmatrix}\Omega_{\mathcal{PP}} & \Omega_{M\mathcal{P}}^{\prime}\\
\Omega_{M\mathcal{P}} & \Omega_{MM}
\end{pmatrix}).
\end{align*}

\end_inset

The term premium is 
\begin_inset Formula 
\begin{align*}
TP_{\tau,t}= & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}(0.5b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{i}+b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\lambda_{\mathcal{P}})\\
 & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}b_{\tau-i}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\sum_{l=1}^{p}\Lambda_{\mathcal{PP},l}\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{P}_{t+i-l}-c]+\sum_{l=1}^{p}\Lambda_{\mathcal{P}M,l}\mathbb{E}_{t}^{\mathbb{P}}[M_{t+i-l}]).
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
If we use annual data so yields are 
\begin_inset Formula $1200\times R_{t}$
\end_inset

,
 the state-space is modified as
\begin_inset Formula 
\begin{align*}
\mathbf{1\mathbf{200\cdot\mathcal{O}_{t}}} & =W_{\mathcal{O}}\mathbf{(1200\cdot R_{t})}=\mathbf{1200\cdot\mathcal{A}_{\mathcal{P}}}+\mathcal{B}_{\mathcal{P}}(\mathbf{1200\cdot\mathcal{P}_{t}}-\mathbf{1200\cdot c})+\mathcal{N}(O_{(N-d_{\mathbb{Q}})\times1},\mathbf{1200^{2}\cdot\Sigma_{\mathcal{O}}}),\\
\mathbf{1200\cdot\mathcal{A}_{\mathcal{P}}} & =W_{\mathcal{O}}[\mathbf{1200\cdot\mathcal{A}_{X}}+\mathcal{B}{}_{X}(\mathbf{1200\cdot\mathcal{T}_{0,\mathcal{P}}})],\\
\mathcal{B}_{\mathcal{P}} & =W_{\mathcal{O}}\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}},\\
\mathcal{T}_{1,\mathcal{P}} & =(W_{\mathcal{P}}\mathcal{B}{}_{X})^{-1},\\
\mathbf{1200\cdot\mathcal{T}_{0,\mathcal{P}}} & =-\mathcal{T}_{1,\mathcal{P}}[\mathbf{W_{\mathcal{P}}(1200\cdot\mathcal{A}_{X})}-\mathbf{1200\cdot c}]=-\mathcal{T}_{1,\mathcal{P}}(\mathbf{1200\cdot\mathcal{T}_{0,X}}),\\
\mathbf{1200\cdot\mathcal{A}{}_{X}^{\prime}} & =\begin{bmatrix}(\mathbf{1200\cdot a_{\tau_{1}}})/\tau_{1} & (\mathbf{1200\cdot a_{\tau_{2}}})/\tau_{2} & \cdots & (\mathbf{1200\cdot a_{\tau_{N}}})/\tau_{N}\end{bmatrix},\\
\mathcal{B}_{X}^{\prime} & =\begin{bmatrix}b_{\tau_{1}}/\tau_{1} & b_{\tau_{2}}/\tau_{2} & \cdots & b_{\tau_{N}}/\tau_{N}\end{bmatrix},\\
\mathbf{1200\cdot a_{\tau}} & =\mathbf{1200\cdot a_{\tau-1}}+b_{\tau-1}^{\prime}(\begin{bmatrix}\mathbf{1200\cdot k_{\infty}^{\mathbb{Q}}}\\
O_{(d_{\mathbb{Q}}-1)\times1}
\end{bmatrix})-\frac{0.5b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathbf{1200^{2}\cdot\Omega_{\mathcal{PP}}})\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1}}{1200},\\
b_{\tau} & =\iota+G_{XX}^{\mathbb{Q}\prime}b_{\tau-1},
\end{align*}

\end_inset

and
\size small

\begin_inset Formula 
\begin{align*}
\begin{bmatrix}\mathbf{1200\cdot\mathcal{P}_{t}}-\mathbf{1200\cdot c}\\
M_{t}
\end{bmatrix}= & \begin{bmatrix}\mathbf{1200\cdot K_{\mathcal{P}}^{\mathbb{P}}}\\
K_{M|\mathcal{F}}^{\mathbb{P}}
\end{bmatrix}+\sum_{l=1}^{p}\begin{pmatrix}G_{\mathcal{PP},l}^{\mathbb{P}} & \mathbf{1200\cdot G_{\mathcal{P}M,l}^{\mathbb{P}}}\\
\mathbf{1200^{-1}\cdot G_{M\mathcal{P},l}^{\mathbb{P}}} & G_{MM,l}^{\mathbb{P}}
\end{pmatrix}\begin{bmatrix}\mathbf{1200\cdot\mathcal{P}_{t-l}-1200\cdot c}\\
M_{t-l}
\end{bmatrix}\\
 & +\mathcal{N}\left(O_{d_{\mathbb{P}}\times1},\begin{pmatrix}\mathbf{1200^{2}\cdot\Omega_{\mathcal{PP}}} & \mathbf{1200\cdot\Omega_{M\mathcal{P}}^{\prime}}\\
\mathbf{1200\cdot\Omega_{M\mathcal{P}}} & \Omega_{MM}
\end{pmatrix}\right).
\end{align*}

\end_inset


\size default
The term premium is modified to
\begin_inset Formula 
\begin{align*}
\mathbf{1200\cdot TP_{\tau,t}}= & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}[\frac{0.5b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathbf{1200^{2}\cdot\Omega_{\mathcal{PP}}})\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{i}}{1200}+b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\mathbf{1200\cdot\lambda_{\mathcal{P}}})]\\
 & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}b_{\tau-i}^{\prime}\mathcal{T}_{1,\mathcal{P}}(\sum_{l=1}^{p}\Lambda_{\mathcal{PP},l}\mathbb{E}_{t}^{\mathbb{P}}[\mathbf{1200\cdot\mathcal{P}_{t+i-l}}-\mathbf{1200\cdot c}]+\sum_{l=1}^{p}\mathbf{1200\cdot\Lambda_{\mathcal{P}M,l}}\mathbb{E}_{t}^{\mathbb{P}}[M_{t+i-l}]),
\end{align*}

\end_inset

and 
\begin_inset Formula 
\[
\mathbf{1200\cdot\lambda_{\mathcal{P}}}=\mathbf{1200\cdot K_{\mathcal{P}}^{\mathbb{P}}}-\mathcal{T}_{1,X}[\begin{bmatrix}\mathbf{1200\cdot k_{\infty}^{\mathbb{Q}}}\\
O_{(d_{\mathbb{Q}}-1)\times1}
\end{bmatrix}+(G_{XX}^{\mathbb{Q}}-I_{d_{\mathbb{Q}}})(\mathbf{1200\cdot\mathcal{T}_{0,\mathcal{P}}})].
\]

\end_inset


\end_layout

\begin_layout Standard
When we redefine the notations for scaled parameters,
 the modified state-space model is almost same as the original.
 Therefore,
 is not irrelevant whether we use annual yields or monthly yields.
 The only thing we have to be careful is the form of the Jensen's inequality term.
 The Jensen's inequality term 
\begin_inset Formula $\mathcal{J}(:\kappa^{\mathbb{Q}},\Omega_{\mathcal{PP}})$
\end_inset

 is
\begin_inset Formula 
\[
\mathcal{J}_{\tau}(:\kappa^{\mathbb{Q}},\Omega_{\mathcal{PP}})=\begin{cases}
0.5b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1}, & \text{if yields are monthly decimal data},\\
\frac{0.5b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Omega_{\mathcal{PP}}\mathcal{T}_{1,\mathcal{P}}^{\prime}b_{\tau-1}}{1200}, & \text{if yields are annual percentage data.}
\end{cases}
\]

\end_inset

Then,
 the difference equation is 
\begin_inset Formula 
\begin{align}
a_{\tau} & =a_{\tau-1}+b_{\tau-1}^{\prime}K_{X}^{\mathbb{Q}}-\mathcal{J}_{\tau}(:\kappa^{\mathbb{Q}},\Omega_{\mathcal{PP}}),\label{eq:sol_a}\\
b_{\tau} & =\iota+G_{XX}^{\mathbb{Q}\prime}b_{\tau-1}.\label{eq:sol_b}
\end{align}

\end_inset

and term premium is 
\begin_inset Formula 
\begin{align*}
TP_{\tau,t}= & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}(\mathcal{J}_{i+1}(:\kappa^{\mathbb{Q}},\Omega_{\mathcal{PP}})+b_{i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\lambda_{\mathcal{P}})\\
 & -\frac{1}{\tau}\sum_{i=1}^{\tau-1}\sum_{l=1}^{p}[b_{\tau-i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Lambda_{\mathcal{PP},l}\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{P}_{t+i-l}-c]+b_{\tau-i}^{\prime}\mathcal{T}_{1,\mathcal{P}}\Lambda_{\mathcal{P}M,l}\mathbb{E}_{t}^{\mathbb{P}}[M_{t+i-l}])].
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The estimated parameters are
\begin_inset Formula 
\[
\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},K_{\mathcal{F}}^{\mathbb{P}},\{G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p},\Omega_{\mathcal{FF}},\Sigma_{\mathcal{O}}.
\]

\end_inset


\end_layout

\begin_layout Subsection
Prior distribution
\end_layout

\begin_layout Subsubsection
VAR covariance 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset


\end_layout

\begin_layout Standard
For a prior distribution of 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

,
 we impose 
\begin_inset Formula 
\[
\Omega_{\mathcal{FF}}\backsim\mathcal{IW}(\nu_{0},diag[\Omega_{0}]).
\]

\end_inset

Instead of using the InverseWishart-InverseWishart update,
 we translate the Wishart distribution into a series of normal-inverse gamma distributions using the following proposition.
\end_layout

\begin_layout Proposition
\begin_inset CommandInset citation
LatexCommand citep
key "chan2022asymmetric"
literal "true"

\end_inset

 Consider the following normal-inverse-gamma priors on the diagonal elements of 
\begin_inset Formula $\Sigma$
\end_inset

 and the lower triangular elements of A:
\begin_inset Formula 
\begin{align*}
\sigma_{i}^{2} & \backsim\mathcal{IG}(\frac{\nu_{0}+i-n}{2},\frac{s_{i}^{2}}{2}),i=1,\cdots,n,\\
(A_{i,j}|\sigma_{i}^{2}) & \backsim\mathcal{N}(0,\frac{\sigma_{i}^{2}}{s_{j}^{2}}),1\leq j<i\leq n,i=2,\cdots,n.
\end{align*}

\end_inset

Then 
\begin_inset Formula $\tilde{\Sigma}^{-1}=A^{\prime}\Sigma^{-1}A$
\end_inset

 has the Wishart distribution 
\begin_inset Formula $\tilde{\Sigma}^{-1}\backsim\mathcal{W}(v_{0},S^{-1})$
\end_inset

,
 where 
\begin_inset Formula $S=diag(s_{1}^{2},s_{2}^{2}\cdots,s_{n}^{2})$
\end_inset

.
 It follows that 
\begin_inset Formula $\tilde{\Sigma}\backsim\mathcal{IW}(\nu_{0},S)$
\end_inset

.
\end_layout

\begin_layout Standard
To this end,
 we orthogonalize the system 
\begin_inset Formula 
\[
\mathcal{F}_{t}=K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t-l}+\epsilon_{\mathcal{F},t}^{\mathbb{P}},
\]

\end_inset

so
\begin_inset Formula 
\begin{equation}
\underbrace{\begin{pmatrix}1 & 0 & 0 & \cdots & 0\\
\mathcal{C}_{21} & 1 & 0 & \cdots & 0\\
\mathcal{C}_{31} & \mathcal{C}_{32} & 1 & \cdots & 0\\
\vdots & \vdots & \vdots & \ddots & \vdots\\
\mathcal{C}_{d_{\mathbb{P}}1} & \mathcal{C}_{d_{\mathbb{P}}2} & \mathcal{C}_{d_{\mathbb{P}}3} & \cdots & 1
\end{pmatrix}}_{\mathcal{C}}\mathcal{F}_{t}=\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t-l}+diag(\underbrace{\begin{bmatrix}\sigma_{\mathcal{FF},1}\\
\vdots\\
\sigma_{\mathcal{FF},d_{\mathbb{P}}}
\end{bmatrix}}_{\tilde{\Omega}_{\mathcal{FF}}^{1/2}})v_{\mathcal{F},t}^{\mathbb{P}}.\label{eq:structural_VAR}
\end{equation}

\end_inset


\begin_inset Formula $\mathcal{C}$
\end_inset

 is implicitly defined as 
\begin_inset Formula $\Omega_{\mathcal{FF}}^{-1}=\mathcal{C}^{\prime}diag[\tilde{\Omega}_{\mathcal{FF}}]^{-1}\mathcal{C}$
\end_inset

 (or 
\begin_inset Formula $\Omega_{\mathcal{FF}}=\mathcal{C}^{-1}diag[\tilde{\Omega}_{\mathcal{FF}}]\mathcal{C}^{-1\prime}$
\end_inset

).
 By the proposition,
 the InverseWishart prior is the same as
\begin_inset Formula 
\begin{align*}
\sigma_{\mathcal{FF},i}^{2} & \backsim\mathcal{IG}(\frac{\nu_{0}+i-d_{\mathbb{P}}}{2},\frac{\Omega_{0,(i)}}{2}),i=1,\cdots,d_{\mathbb{P}},\\
\mathcal{C}_{ij}|\sigma_{\mathcal{FF},i}^{2} & \backsim\mathcal{N}(0,\frac{\sigma_{\mathcal{FF},i}^{2}}{\Omega_{0,(j)}}),1\leq j<i\leq d_{\mathbb{P}},i=2,\cdots,d_{\mathbb{P}}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
From the recursive structure that 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

 has,
 we can derive that
\begin_inset Formula 
\begin{align*}
\Omega_{\mathcal{PP}} & =(\mathcal{C}^{-1})_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})}diag[\tilde{\Omega}_{\mathcal{FF},(1:d_{\mathbb{Q}})}]((\mathcal{C}^{-1})_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{\prime}.
\end{align*}

\end_inset

Because 
\begin_inset Formula $\mathcal{C}$
\end_inset

 is a lower triangular matrix,
 when we partition 
\begin_inset Formula 
\begin{align*}
\mathcal{C} & =\begin{pmatrix}\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\\
\mathcal{C}_{(d_{\mathbb{Q}}+1:d_{\mathbb{P}},1:d_{\mathbb{Q}})} & \mathcal{C}_{(d_{\mathbb{Q}}+1:d_{\mathbb{P}},d_{\mathbb{Q}}+1:d_{\mathbb{P}})}
\end{pmatrix},\\
\mathcal{C}^{-1} & =\begin{pmatrix}(\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{-1} & O_{d_{\mathbb{Q}}\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\\
-(\mathcal{C}_{(d_{\mathbb{Q}}+1:d_{\mathbb{P}},d_{\mathbb{Q}}+1:d_{\mathbb{P}})})^{-1}\mathcal{C}_{(d_{\mathbb{Q}}+1:d_{\mathbb{P}},1:d_{\mathbb{Q}})}(\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{-1} & (\mathcal{C}_{(d_{\mathbb{Q}}+1:d_{\mathbb{P}},d_{\mathbb{Q}}+1:d_{\mathbb{P}})})^{-1}
\end{pmatrix},\\
(\mathcal{C}^{-1})_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})} & =(\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{-1}.\\
\therefore\Omega_{\mathcal{PP}} & =(\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{-1}diag[\tilde{\Omega}_{\mathcal{FF},(1:d_{\mathbb{Q}})}](\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{-1\prime}.
\end{align*}

\end_inset

In terms of a precision,
 
\begin_inset Formula 
\[
\Omega_{\mathcal{PP}}^{-1}=(\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})})^{\prime}diag[\tilde{\Omega}_{\mathcal{FF},(1:d_{\mathbb{Q}})}]^{-1}\mathcal{C}_{(1:d_{\mathbb{Q}},1:d_{\mathbb{Q}})}.
\]

\end_inset

Therefore,
 sampling 
\begin_inset Formula $\sigma_{\mathcal{FF},1}^{2}$
\end_inset

,
 and 
\begin_inset Formula $\{C_{(i,1:i-1)},\sigma_{\mathcal{FF},i}^{2}\}_{i=2}^{d_{\mathbb{Q}}}$
\end_inset

 is equal to sampling 
\begin_inset Formula $\Omega_{\mathcal{PP}}$
\end_inset

.
 For information,
 
\begin_inset Formula 
\[
\Omega_{\mathcal{PP}}\backsim\mathcal{IW}(\nu_{0}-(d_{\mathbb{P}}-d_{\mathbb{Q}}),diag[\Omega_{0,(1:d_{\mathbb{Q}})}]).
\]

\end_inset


\end_layout

\begin_layout Subsubsection
VAR conditional mean 
\begin_inset Formula $K_{\mathcal{F}}^{\mathbb{P}}$
\end_inset

 and 
\begin_inset Formula $\{G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p}$
\end_inset


\end_layout

\begin_layout Standard
First,
 we set prior means of 
\begin_inset Formula $\{G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p}$
\end_inset

.
 We set prior mean of 
\begin_inset Formula $G_{\mathcal{PP},1}^{\mathbb{P}}$
\end_inset

 to the prior mean of 
\begin_inset Formula $G_{\mathcal{PP}}^{\mathbb{Q}}$
\end_inset

.
 For other diagonal elements in 
\begin_inset Formula $G_{\mathcal{PP},1}^{\mathbb{P}}$
\end_inset

,
 we set to zero and 0.9 for growth and level variables,
 respectively.
 The other elements in 
\begin_inset Formula $\{G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p}$
\end_inset

 have zero prior means.
 
\end_layout

\begin_layout Standard
For the efficiency of the sampler,
 we estimate the orthogonalized equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:structural_VAR"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

).
 Since 
\begin_inset Formula 
\[
(\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}})_{(i,j)}=G_{\mathcal{FF},l,(i,j)}^{\mathbb{P}}+\sum_{k=1}^{i-1}\mathcal{C}_{ik}(G_{\mathcal{FF},l}^{\mathbb{P}})_{(k,j)},
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\mathbb{E}[(\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}})_{(i,j)}] & =\mathbb{E}[\mathbb{E}[(\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}})_{(i,j)}|G_{\mathcal{FF},l}^{\mathbb{P}},\tilde{\Omega}_{\mathcal{FF}}]],\\
 & =\mathbb{E}[\mathbb{E}[G_{\mathcal{FF},l,(i,j)}^{\mathbb{P}}+\sum_{k=1}^{i-1}\mathcal{C}_{ik}(G_{\mathcal{FF},l}^{\mathbb{P}})_{(k,j)}|G_{\mathcal{FF},l}^{\mathbb{P}},\tilde{\Omega}_{\mathcal{FF}}]],\\
 & =\mathbb{E}[G_{\mathcal{FF},l,(i,j)}^{\mathbb{P}}]
\end{align*}

\end_inset


\begin_inset CommandInset citation
LatexCommand citep
key "chan2022asymmetric"
literal "true"

\end_inset

.
 Therefore,
 we can use prior means of 
\begin_inset Formula $\{G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p}$
\end_inset

 as prior means of 
\begin_inset Formula $\{\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p}$
\end_inset

.
 For the intercept term,
 it is 
\begin_inset Formula 
\[
(\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}})_{(i)}=K_{\mathcal{F},(i)}^{\mathbb{P}}+\sum_{k=1}^{i-1}\mathcal{C}_{ik}(K_{\mathcal{F}}^{\mathbb{P}})_{(k)},
\]

\end_inset

and the corresponding prior mean is
\begin_inset Formula 
\begin{align*}
\mathbb{E}[(\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}})_{(i)}] & =\mathbb{E}[\mathbb{E}[K_{\mathcal{F},(i)}^{\mathbb{P}}+\sum_{k=1}^{i-1}\mathcal{C}_{ik}(K_{\mathcal{F}}^{\mathbb{P}})_{(k)}|K_{\mathcal{F}}^{\mathbb{P}},\tilde{\Omega}_{ff}]],\\
 & =\mathbb{E}[K_{\mathcal{F},(i)}^{\mathbb{P}}].
\end{align*}

\end_inset

Therefore,
 we can use a reduced form prior for 
\begin_inset Formula $K_{\mathcal{F}}^{\mathbb{P}}$
\end_inset

 as a prior for 
\begin_inset Formula $\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}}$
\end_inset

.
 Here,
 we set a mean prior of 
\begin_inset Formula $K_{\mathcal{P}}^{\mathbb{P}}$
\end_inset

 to be same to that of 
\begin_inset Formula $K_{\mathcal{P}}^{\mathbb{Q}}$
\end_inset

.
 We can calculate a prior mean of 
\begin_inset Formula $K_{\mathcal{P}}^{\mathbb{Q}}$
\end_inset

 with a Monte Carlo simulation.
 The prior mean of the remaining elements in 
\begin_inset Formula $K_{\mathcal{F}}^{\mathbb{P}}$
\end_inset

 is set to zero.
\end_layout

\begin_layout Standard
A remaining thing is to set prior variances.
 We follow 
\begin_inset CommandInset citation
LatexCommand cite
key "chan2021minnesotatype"
literal "false"

\end_inset

,
 so our prior variances are
\begin_inset Formula 
\begin{align*}
\frac{Var[(\mathcal{C}G_{\mathcal{FF},l}^{\mathbb{P}})_{(i,j)}]}{\sigma_{\mathcal{FF},i}^{2}} & =\underbrace{\psi_{l,ij}}_{local}\cdot\underbrace{\frac{q_{slope}}{l^{q_{lag}}}\frac{(\nu_{0}-d_{\mathbb{P}}-1)}{\Omega_{0,(j)}}}_{Minnesota},\\
\text{where }q_{slope} & =\begin{cases}
q_{11} & \text{if }i=j\text{ and }i\leq d_{\mathbb{Q}},\\
q_{12} & \text{if }i=j\text{ and }i>d_{\mathbb{Q}},\\
q_{21} & \text{if }i\neq j\text{ and }i\leq d_{\mathbb{Q}},\\
q_{22} & \text{otherwise,}
\end{cases}\\
q_{lag} & =\begin{cases}
q_{31} & \text{if }i\leq d_{\mathbb{Q}},\\
q_{32} & \text{otherwise},
\end{cases}
\end{align*}

\end_inset

and 
\begin_inset Formula $\psi_{l,ij}\backsim\text{\ensuremath{\mathcal{G}(\eta_{\psi},\eta_{\psi})}}$
\end_inset

 or 
\begin_inset Formula $\psi_{l,ij}=1$
\end_inset

 if we want to shut down a local shrinkage.
 The benchmark value for 
\begin_inset Formula $q_{lag}$
\end_inset

 is two.
 If we introduce the local prior,
\begin_inset Formula 
\[
\eta_{\psi}\backsim\mathcal{G}(1,1)
\]

\end_inset

that reminds us that 
\begin_inset Formula $\eta_{\psi}=1$
\end_inset

 corresponds to the Bayesian Lasso 
\begin_inset CommandInset citation
LatexCommand citep
key "park2008thebayesian"
literal "false"

\end_inset

.
 In the case of the intercept,
\begin_inset Formula 
\begin{align*}
\frac{Var[(\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}})_{(i)}]}{\sigma_{\mathcal{FF},i}^{2}} & =\underbrace{\psi_{0,i}}_{local}\cdot\underbrace{q_{intercept}}_{Minnesota},\\
\text{where }q_{intercept} & =\begin{cases}
q_{41} & \text{if }i\leq d_{\mathbb{Q}},\\
q_{42} & \text{otherwise}.
\end{cases}
\end{align*}

\end_inset

The intercept and slope matrices are estimated with 
\begin_inset Formula $\mathcal{C}$
\end_inset

.
 Specifically,
 for 
\begin_inset Formula 
\begin{align*}
\phi_{i} & =\begin{bmatrix}\begin{bmatrix}(\mathcal{C}K_{\mathcal{F}}^{\mathbb{P}})_{(i)} & (\mathcal{C}G_{\mathcal{FF},1}^{\mathbb{P}})_{(i,:)} & \cdots & (\mathcal{C}G_{\mathcal{FF},p}^{\mathbb{P}})_{(i,:)}\end{bmatrix} & \begin{bmatrix}\mathcal{C}_{i1} & \cdots & \mathcal{C}_{i(i-1)}\end{bmatrix}\end{bmatrix}^{\prime},
\end{align*}

\end_inset

we express the 
\begin_inset Formula $i$
\end_inset

th row of orthogonalized system (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:structural_VAR"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

) as
\begin_inset Formula 
\[
\underbrace{\begin{bmatrix}\mathcal{F}_{1,(i)}\\
\mathcal{F}_{2,(i)}\\
\vdots\\
\mathcal{F}_{T,(i)}
\end{bmatrix}}_{y_{\phi_{i}}}=X_{\phi_{i}}\phi_{i}+\mathcal{N}(O_{T\times1},\sigma_{\mathcal{FF},i}^{2}I_{T}).
\]

\end_inset

By estimating 
\begin_inset Formula $\phi_{i}$
\end_inset

 and 
\begin_inset Formula $\sigma_{\mathcal{FF},i}^{2}$
\end_inset

 
\begin_inset Formula $\forall i$
\end_inset

,
 we can derive a posterior sample of 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

,
 
\begin_inset Formula $K_{\mathcal{F}}^{\mathbb{P}}$
\end_inset

 and 
\begin_inset Formula $\{G_{\mathcal{FF},l}^{\mathbb{P}}\}_{l=1}^{p}$
\end_inset

.
 The corresponding prior distribution is 
\begin_inset Formula 
\[
\phi_{i}\backsim\mathcal{N}(m_{i},\sigma_{\mathcal{FF},i}^{2}\cdot diag[V_{i}]).
\]

\end_inset


\end_layout

\begin_layout Subsubsection
prices of risks 
\begin_inset Formula $k_{\infty}^{\mathbb{Q}}$
\end_inset


\end_layout

\begin_layout Standard
We observe 
\begin_inset Formula $\mathbb{P}$
\end_inset

-dynamics only once at a time,
 but we can observe 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-dynamics several times contemporaneously through forward rates in the bond market.
 Therefore,
 
\begin_inset Formula $\mathbb{Q}$
\end_inset

 dynamics is estimated stably without a sensitivity on prior settings.
 We impose 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
k_{\infty}^{\mathbb{Q}} & \backsim\mathcal{N}(0,\sigma_{k_{\infty}^{\mathbb{Q}}}^{2}),\\
\sigma_{k_{\infty}^{\mathbb{Q}}}^{2} & =0.01^{2}.
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Decay parameter 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

 
\end_layout

\begin_layout Standard
The DNS decay parameter is typically chosen to maximize the curvature factor loading at a specific maturity.
 The curvature factor loading is 
\begin_inset Formula 
\[
\frac{1-\exp(-\kappa^{\mathbb{Q}}\tau)}{\kappa^{\mathbb{Q}}\tau}-\exp(-\kappa^{\mathbb{Q}}\tau).
\]

\end_inset

We calculate 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

 that maximize the curvature factor loading at each maturity 
\begin_inset Formula $\tau\in\{24,30,\cdots,60\}$
\end_inset

.
 Suppose the calculated decay parameters are 
\begin_inset Formula $\{\kappa_{24}^{\mathbb{Q}},\kappa_{30}^{\mathbb{Q}}\cdots,\kappa_{60}^{\mathbb{Q}}\}$
\end_inset

.
 We impose a uniform distribution on grid 
\begin_inset Formula $\{\kappa_{24}^{\mathbb{Q}},\kappa_{30}^{\mathbb{Q}}\cdots,\kappa_{60}^{\mathbb{Q}}\}$
\end_inset

 as a prior distribution for 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

.
 A first derivative of the factor loading w.r.t.
 
\begin_inset Formula $\tau$
\end_inset

 is
\begin_inset Formula 
\[
\frac{(\kappa^{\mathbb{Q}}\tau+1)\exp(-\kappa^{\mathbb{Q}}\tau)-1}{\kappa^{\mathbb{Q}}\tau^{2}}+\exp(-\kappa^{\mathbb{Q}}\tau)\kappa^{\mathbb{Q}}.
\]

\end_inset

On the grid,
 the derivative should be zero for each maturity 
\begin_inset Formula $\tau\in\{24,30,\cdots,60\}$
\end_inset

.
\end_layout

\begin_layout Subsubsection
Pricing error 
\begin_inset Formula $\Sigma_{\mathcal{O}}$
\end_inset


\end_layout

\begin_layout Standard
We impose a hierarchical prior structure,
 so that 
\begin_inset Formula 
\begin{align*}
\sigma_{\mathcal{O},i}^{2} & \backsim\mathcal{IG}(2,\gamma_{i}),\\
\gamma_{i} & \backsim\mathcal{G}(1,\bar{\gamma}).
\end{align*}

\end_inset

To determine 
\begin_inset Formula $\bar{\gamma}$
\end_inset

,
 we regress the measurement equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:mea"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

 using OLS with unrestricted coefficients.
 And then,
 we set an inverse of 
\begin_inset Formula $\bar{\gamma}$
\end_inset

 to an average of the residual variances.
 
\end_layout

\begin_layout Subsubsection
Summary
\end_layout

\begin_layout Standard
Our prior distribution is 
\begin_inset Formula 
\begin{align*}
 & p(\theta\equiv\{\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{P}}},\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}},\eta_{\psi},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}},\Sigma_{\mathcal{O}},\{\gamma_{i}\}_{i=1}^{N-d_{\mathbb{Q}}}\}|p,\nu_{0},\Omega_{0},q)\\
= & p(\{\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{P}}}|\nu_{0},\Omega_{0})p(\{\phi_{i}\}_{i=1}^{d_{\mathbb{P}}}|p,\{\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{P}}},q,\nu_{0},\Omega_{0},\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}})\\
 & \times p(\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}}|p,\eta_{\psi})p(\eta_{\psi})p(k_{\infty}^{\mathbb{Q}})p(\kappa^{\mathbb{Q}})p(\Sigma_{\mathcal{O}}|\{\gamma_{i}\}_{i=1}^{N-d_{\mathbb{Q}}})p(\{\gamma_{i}\}_{i=1}^{N-d_{\mathbb{Q}}}),
\end{align*}

\end_inset

where 
\begin_inset Formula $q\equiv\begin{bmatrix}q_{slope} & q_{lag} & q_{intercept}\end{bmatrix}^{\prime}$
\end_inset

.
 The hyper-parameters are 
\begin_inset Formula $p$
\end_inset

,
 
\begin_inset Formula $\nu_{0}$
\end_inset

,
 
\begin_inset Formula $\Omega_{0}$
\end_inset

,
 and 
\begin_inset Formula $q$
\end_inset

.
 Our MH block contains 
\begin_inset Formula $\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{Q}}}$
\end_inset

 and 
\begin_inset Formula $\eta_{\psi}$
\end_inset

.
\end_layout

\begin_layout Subsection
Likelihood
\end_layout

\begin_layout Standard
Our likelihood function is
\begin_inset Formula 
\begin{align*}
p(\{\mathcal{O}_{t},\mathcal{P}_{t},M_{t}\}_{t=1}^{T}|\theta) & =\Pi_{t=1}^{T}p(\mathcal{O}_{t},\mathcal{P}_{t},M_{t}|\mathcal{I}_{t-1},\theta),\\
p(\mathcal{O}_{t},\mathcal{P}_{t},M_{t}|\mathcal{I}_{t-1},\theta) & =p(\mathcal{O}_{t}|\mathcal{P}_{t},\theta)p(\mathcal{F}_{t}|\mathcal{I}_{t-1},\theta).
\end{align*}

\end_inset

We express the likelihood in terms of orthogonalized system (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:structural_VAR"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

),
 so
\begin_inset Formula 
\begin{align*}
p(\mathcal{F}_{t,(1)},\cdots,\mathcal{F}_{t,(d_{\mathbb{P}})}|\mathcal{I}_{t-1},\theta) & =p(\mathcal{F}_{t,(1)}|\mathcal{I}_{t-1},\theta)\Pi_{i=2}^{d_{\mathbb{P}}}p(\mathcal{F}_{t,(i)}|\mathcal{F}_{t,(i-1)},\cdots,\mathcal{F}_{t,(1)},\mathcal{I}_{t-1},\theta),
\end{align*}

\end_inset

Therefore,
\begin_inset Formula 
\begin{align*}
p(\{\mathcal{O}_{t},\mathcal{P}_{t},M_{t}\}_{t=1}^{T}|\theta) & =\Pi_{t=1}^{T}p(\mathcal{O}_{t}|\mathcal{P}_{t},\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}},\Sigma_{\mathcal{O}})\\
 & \times\Pi_{t=1}^{T}p(\mathcal{F}_{t,(1)}|\mathcal{I}_{t-1},\phi_{1},\sigma_{\mathcal{FF},1}^{2})\\
 & \times\Pi_{i=2}^{d_{\mathbb{P}}}[\Pi_{t=1}^{T}p(\mathcal{F}_{t,(i)}|\mathcal{F}_{t,(i-1)},\cdots,\mathcal{F}_{t,(1)},\mathcal{I}_{t-1},\phi_{i},\sigma_{\mathcal{FF},i}^{2})].
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The measurement equation is 
\begin_inset Formula 
\[
\Pi_{t=1}^{T}p(\mathcal{O}_{t}|\mathcal{P}_{t},\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}},\Sigma_{\mathcal{O}})=\Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t}-c),\Sigma_{\mathcal{O}}),
\]

\end_inset

and the transition equation is 
\begin_inset Formula 
\[
\Pi_{t=1}^{T}p(\mathcal{F}_{t,(i)}|\mathcal{F}_{t,(i-1)},\cdots,\mathcal{F}_{t,(1)},\mathcal{I}_{t-1},\theta)=\mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T}).
\]

\end_inset

Therefore,
\begin_inset Formula 
\begin{align*}
p(\{\mathcal{O}_{t},\mathcal{P}_{t},M_{t}\}_{t=1}^{T}|\theta)= & \Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t}-c),\Sigma_{\mathcal{O}})\\
 & \times\Pi_{i=1}^{d_{\mathbb{P}}}\mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T}).
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Posterior distribution
\end_layout

\begin_layout Standard
We use the Gibbs sampling to sampling our joint posterior distribution.
 
\end_layout

\begin_layout Subsubsection
Long-run level parameter 
\begin_inset Formula $k_{\infty}^{\mathbb{Q}}$
\end_inset


\end_layout

\begin_layout Standard
The full-conditional posterior distribution is 
\begin_inset Formula 
\begin{align*}
p(k_{\infty}^{\mathbb{Q}}|\mathcal{I}_{T},\theta\backslash\{k_{\infty}^{\mathbb{Q}}\}) & \propto p(k_{\infty}^{\mathbb{Q}})\Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t}-c),\Sigma_{\mathcal{O}}).
\end{align*}

\end_inset

We first simplify the measurement equation.
 A close inspection on the difference equations 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:sol_a"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:sol_b"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

 leads to
\begin_inset Formula 
\begin{align*}
a_{\tau} & =a_{\tau-1}-\mathcal{J}_{\tau}(:\kappa^{\mathbb{Q}},\Omega_{\mathcal{PP}})+(\tau-1)k_{\infty}^{\mathbb{Q}}.
\end{align*}

\end_inset

So,
\begin_inset Formula 
\begin{align*}
a_{\tau} & =0,\text{for }\tau<2\\
a_{\tau} & =\underbrace{-\sum_{i=2}^{\tau}\mathcal{J}_{i}(:\kappa^{\mathbb{Q}},\Omega_{\mathcal{PP}})}_{a_{0\tau}}+\underbrace{\sum_{i=2}^{\tau}(i-1)}_{a_{1\tau}}k_{\infty}^{\mathbb{Q}},\text{for }\tau\geq2,
\end{align*}

\end_inset

and,
\begin_inset Formula 
\[
\mathcal{A}_{X}=\underbrace{\begin{bmatrix}a_{0\tau_{1}}/\tau_{1}\\
a_{0\tau_{2}}/\tau_{2}\\
\vdots\\
a_{0\tau_{N}}/\tau_{N}
\end{bmatrix}}_{\mathcal{A}_{0,k_{\infty}^{\mathbb{Q}}}}+\underbrace{\begin{bmatrix}\sum_{i=2}^{\tau_{1}}\frac{(i-1)}{\tau_{1}}\\
\sum_{i=2}^{\tau_{2}}\frac{(i-1)}{\tau_{2}}\\
\vdots\\
\sum_{i=2}^{\tau_{N}}\frac{(i-1)}{\tau_{N}}
\end{bmatrix}}_{\mathcal{A}_{1,k_{\infty}^{\mathbb{Q}}}}k_{\infty}^{\mathbb{Q}}.
\]

\end_inset

Based on the result,
 the measurement equation can be rephrased as
\begin_inset Formula 
\begin{align*}
\mathcal{O}_{t} & =\mathcal{A}_{\mathcal{P}}+\mathcal{B}_{\mathcal{P}}(\mathcal{P}_{t}-c)+\mathcal{N}(O_{N-d_{\mathbb{Q}}},\Sigma_{\mathcal{O}}),\\
 & =W_{\mathcal{O}}(\mathcal{A}_{X}+\mathcal{B}{}_{X}\mathcal{T}_{0,\mathcal{P}})+\mathcal{B}_{\mathcal{P}}(\mathcal{P}_{t}-c)+\mathcal{N}(O_{N-d_{\mathbb{Q}}},\Sigma_{\mathcal{O}}),\\
 & =W_{\mathcal{O}}(I_{N}-\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}W_{\mathcal{P}})\mathcal{A}_{X}+W_{\mathcal{O}}\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}c+\mathcal{B}_{\mathcal{P}}(\mathcal{P}_{t}-c)+\mathcal{N}(O_{N-d_{\mathbb{Q}}},\Sigma_{\mathcal{O}}),\\
 & =W_{\mathcal{O}}(I_{N}-\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}W_{\mathcal{P}})(\mathcal{A}_{0,k_{\infty}^{\mathbb{Q}}}+\mathcal{A}_{1,k_{\infty}^{\mathbb{Q}}}k_{\infty}^{\mathbb{Q}})+W_{\mathcal{O}}\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}c+\mathcal{B}_{\mathcal{P}}(\mathcal{P}_{t}-c)+\mathcal{N}(O_{N-d_{\mathbb{Q}}},\Sigma_{\mathcal{O}}),
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\therefore & \underbrace{\Sigma_{\mathcal{O}}^{-1/2}[\mathcal{O}_{t}-W_{\mathcal{O}}(I_{N}-\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}W_{\mathcal{P}})\mathcal{A}_{0,k_{\infty}^{\mathbb{Q}}}-W_{\mathcal{O}}\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}c-\mathcal{B}_{\mathcal{P}}(\mathcal{P}_{t}-c)]}_{y_{k_{\infty}^{\mathbb{Q}},t}}\\
 & =\underbrace{\Sigma_{\mathcal{O}}^{-1/2}W_{\mathcal{O}}(I_{N}-\mathcal{B}{}_{X}\mathcal{T}_{1,\mathcal{P}}W_{\mathcal{P}})\mathcal{A}_{1,k_{\infty}^{\mathbb{Q}}}}_{x_{k_{\infty}^{\mathbb{Q}}}}k_{\infty}^{\mathbb{Q}}+\mathcal{N}(O_{(N-d_{\mathbb{Q}})\times1},I_{N-d_{\mathbb{Q}}}).
\end{align*}

\end_inset

 Stacking up 
\begin_inset Formula $y_{k_{\infty}^{\mathbb{Q}},t}$
\end_inset

 leads to 
\begin_inset Formula 
\[
\underbrace{\begin{bmatrix}y_{k_{\infty}^{\mathbb{Q}},1}\\
y_{k_{\infty}^{\mathbb{Q}},2}\\
\vdots\\
y_{k_{\infty}^{\mathbb{Q}},T}
\end{bmatrix}}_{Y_{k_{\infty}^{\mathbb{Q}}}}=\underbrace{\begin{bmatrix}x_{k_{\infty}^{\mathbb{Q}}}\\
x_{k_{\infty}^{\mathbb{Q}}}\\
\vdots\\
x_{k_{\infty}^{\mathbb{Q}}}
\end{bmatrix}}_{X_{k_{\infty}^{\mathbb{Q}}}}k_{\infty}^{\mathbb{Q}}+\mathcal{N}(O_{T(N-d_{\mathbb{Q}})\times1},I_{T(N-d_{\mathbb{Q}})}).
\]

\end_inset

This derivation says that we can do the Normal-Normal update for 
\begin_inset Formula $k_{\infty}^{\mathbb{Q}}$
\end_inset

,
 so
\begin_inset Formula 
\[
p(k_{\infty}^{\mathbb{Q}}|\mathcal{I}_{T},\theta\backslash\{k_{\infty}^{\mathbb{Q}}\})=\mathcal{N}((X_{k_{\infty}^{\mathbb{Q}}}^{\prime}X_{k_{\infty}^{\mathbb{Q}}}+\sigma_{k_{\infty}^{\mathbb{Q}}}^{-2})^{-1}X_{k_{\infty}^{\mathbb{Q}}}^{\prime}Y_{k_{\infty}^{\mathbb{Q}}},(X_{k_{\infty}^{\mathbb{Q}}}^{\prime}X_{k_{\infty}^{\mathbb{Q}}}+\sigma_{k_{\infty}^{\mathbb{Q}}}^{-2})^{-1}).
\]

\end_inset

We do not care about the Jacobian term because it does not depend on 
\begin_inset Formula $k_{\infty}^{\mathbb{Q}}$
\end_inset

.
\end_layout

\begin_layout Subsubsection
Decay parameter 
\begin_inset Formula $\kappa^{\mathbb{Q}}$
\end_inset

 
\end_layout

\begin_layout Standard
The unnormalized kernel of the full-conditional posterior distribution is 
\begin_inset Formula 
\begin{align*}
p^{*}(\kappa^{\mathbb{Q}}=\kappa_{i}^{\mathbb{Q}}|\mathcal{I}_{T},\theta\backslash\{\kappa^{\mathbb{Q}}\})= & p(\kappa_{i}^{\mathbb{Q}})\Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa_{i}^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa_{i}^{\mathbb{Q}})(\mathcal{P}_{t}-c),\Sigma_{\mathcal{O}}).
\end{align*}

\end_inset

Therefore,
 the posterior is
\begin_inset Formula 
\[
p(\kappa^{\mathbb{Q}}=\kappa_{i}^{\mathbb{Q}}|\mathcal{I}_{T},\theta\backslash\{\kappa^{\mathbb{Q}}\})=\frac{p^{*}(\kappa^{\mathbb{Q}}=\kappa_{i}^{\mathbb{Q}}|\mathcal{I}_{T},\theta\backslash\{\kappa^{\mathbb{Q}}\})}{\sum_{j\in\{24,30,\cdots,60\}}p^{*}(\kappa^{\mathbb{Q}}=\kappa_{j}^{\mathbb{Q}}|\mathcal{I}_{T},\theta\backslash\{\kappa^{\mathbb{Q}}\})},
\]

\end_inset

for 
\begin_inset Formula $i\in\{24,30,\cdots,60\}.$
\end_inset


\end_layout

\begin_layout Subsubsection
MCMC algorithm for 
\begin_inset Formula $\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{Q}}}$
\end_inset

,
 and 
\begin_inset Formula $\eta_{\psi}$
\end_inset


\end_layout

\begin_layout Standard
The full-conditional posterior is 
\begin_inset Formula 
\begin{align*}
 & \Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t}-c),\Sigma_{\mathcal{O}})\times\Pi_{i=1}^{d_{\mathbb{P}}}\mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T})\\
\times & p(\{\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{P}}}|\nu_{0},\Omega_{0})p(\{\phi_{i}\}_{i=1}^{d_{\mathbb{P}}}|p,\{\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{P}}},q,\nu_{0},\Omega_{0},\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}})\\
\times & \Pi_{l=1}^{p}\Pi_{i=1}^{d_{\mathbb{P}}}\Pi_{j=1}^{d_{\mathbb{P}}}\text{\ensuremath{\mathcal{G}(\psi_{l,ij}|\eta_{\psi},\eta_{\psi})}}\times\Pi_{i=1}^{d_{\mathbb{P}}}\text{\ensuremath{\mathcal{G}(\psi_{0,i}|\eta_{\psi},\eta_{\psi})}}\times\mathcal{G}(\eta_{\psi}|1,1).
\end{align*}

\end_inset

This MCMC sampling is divided into 
\begin_inset Formula $d_{\mathbb{Q}}+1$
\end_inset

 MCMC blocks,
 that are 
\begin_inset Formula $\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}$
\end_inset

 for each 
\begin_inset Formula $i\in\{1,\cdots,d_{\mathbb{Q}}\}$
\end_inset

 and 
\begin_inset Formula $\eta_{\psi}$
\end_inset

.
\end_layout

\begin_layout Paragraph
Sampling 
\begin_inset Formula $\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}_{i=1}^{d_{\mathbb{Q}}}$
\end_inset


\end_layout

\begin_layout Standard
For specific 
\begin_inset Formula $i$
\end_inset

,
 the targeted 
\begin_inset Formula $\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}$
\end_inset

 has a posterior distribution of
\begin_inset Formula 
\begin{align*}
 & \Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP}})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})(\mathcal{P}_{t}-c),\Sigma_{\mathcal{O}})\\
\times & \mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T})\times\mathcal{N}(\phi_{i}|m_{i},\sigma_{\mathcal{FF},i}^{2}\cdot diag[V_{i}])\times\mathcal{IG}(\sigma_{\mathcal{FF},i}^{2}|\frac{\nu_{0}+i-d_{\mathbb{P}}}{2},\frac{\Omega_{0,(i)}}{2}).
\end{align*}

\end_inset

Here,
 
\begin_inset Formula 
\[
\mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T})\times\mathcal{N}(\phi_{i}|m_{i},\sigma_{\mathcal{FF},i}^{2}\cdot diag[V_{i}])\times\mathcal{IG}(\sigma_{\mathcal{FF},i}^{2}|\frac{\nu_{0}+i-d_{\mathbb{P}}}{2},\frac{\Omega_{0,(i)}}{2})
\]

\end_inset

corresponds to the Normal-InverseGamma-Normal-InverseGamma update.
 We set it as a proposal distribution.
 Then,
 the acceptance probability is 
\begin_inset Formula 
\[
min[1,\frac{\Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP},proposal})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})\mathcal{P}_{t},\Sigma_{\mathcal{O}})}{\Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t}|\mathcal{A}_{\mathcal{P}}(:\kappa^{\mathbb{Q}},k_{\infty}^{\mathbb{Q}},\Omega_{\mathcal{PP},current})+\mathcal{B}_{\mathcal{P}}(:\kappa^{\mathbb{Q}})\mathcal{P}_{t},\Sigma_{\mathcal{O}})}].
\]

\end_inset


\end_layout

\begin_layout Paragraph
Sampling 
\begin_inset Formula $\eta_{\psi}$
\end_inset


\end_layout

\begin_layout Standard
The log target distribution is 
\begin_inset Formula 
\begin{align*}
 & \log p(\eta_{\psi}|\mathcal{I}_{T},\theta\backslash\{\eta_{\psi}\})\\
= & \sum_{l=1}^{p}\sum_{i=1}^{d_{\mathbb{P}}}\sum_{j=1}^{d_{\mathbb{P}}}\log\text{\ensuremath{\mathcal{G}(\psi_{l,ij}|\eta_{\psi},\eta_{\psi})}}+\sum_{i=1}^{d_{\mathbb{P}}}\log\text{\ensuremath{\mathcal{G}(\psi_{0,i}|\eta_{\psi},\eta_{\psi})}}+\log\mathcal{G}(\eta_{\psi}|1,1)+\text{constant},\\
= & \sum_{l=1}^{p}\sum_{i=1}^{d_{\mathbb{P}}}\sum_{j=1}^{d_{\mathbb{P}}}\log[\frac{\eta_{\psi}^{\eta_{\psi}}}{\Gamma(\eta_{\psi})}\psi_{l,ij}^{\eta_{\psi}-1}\exp[-\eta_{\psi}\psi_{l,ij}]]\\
 & +\sum_{i=1}^{d_{\mathbb{P}}}\log[\frac{\eta_{\psi}^{\eta_{\psi}}}{\Gamma(\eta_{\psi})}\psi_{0,i}^{\eta_{\psi}-1}\exp[-\eta_{\psi}\psi_{0,i}]]+\log[\frac{1}{\Gamma(1)}\exp[-\eta_{\psi}]]+\text{constant}\\
= & \sum_{l=1}^{p}\sum_{i=1}^{d_{\mathbb{P}}}\sum_{j=1}^{d_{\mathbb{P}}}[\eta_{\psi}\log\eta_{\psi}-\log\Gamma(\eta_{\psi})+(\eta_{\psi}-1)\log\psi_{l,ij}-\eta_{\psi}\psi_{l,ij}]\\
 & +\sum_{i=1}^{d_{\mathbb{P}}}[\eta_{\psi}\log\eta_{\psi}-\log\Gamma(\eta_{\psi})+(\eta_{\psi}-1)\log\psi_{0,i}-\eta_{\psi}\psi_{0,i}]-\log\Gamma(1)-\eta_{\psi}+\text{constant},\\
= & (pd_{\mathbb{P}}^{2}+d_{\mathbb{P}})[\eta_{\psi}\log\eta_{\psi}-\log\Gamma(\eta_{\psi})]+\sum_{\forall\psi}[(\eta_{\psi}-1)\log\psi-\eta_{\psi}\psi]-\log\Gamma(1)-\eta_{\psi}+\text{constant}.
\end{align*}

\end_inset

We derive an approximated target distribution and then use it as a proposal distribution.
 The first derivative is 
\begin_inset Formula 
\[
\frac{\partial\log p(\eta_{\psi}|\mathcal{I}_{T},\theta\backslash\{\eta_{\psi}\})}{\partial\eta_{\psi}}=(pd_{\mathbb{P}}^{2}+d_{\mathbb{P}})[\log\eta_{\psi}+1-\frac{d\log\Gamma(\eta_{\psi})}{d\eta_{\psi}}]+\sum_{\forall\psi}\log\psi-\sum_{\forall\psi}\psi-1,
\]

\end_inset

where 
\begin_inset Formula $\frac{d\log\Gamma(\eta_{\psi})}{d\eta_{\psi}}$
\end_inset

 is a digamma function.
 The second derivative is 
\begin_inset Formula 
\[
\frac{\partial^{2}\log p(\eta_{\psi}|\mathcal{I}_{T},\theta\backslash\{\eta_{\psi}\})}{(\partial\eta_{\psi})^{2}}=(pd_{\mathbb{P}}^{2}+d_{\mathbb{P}})[\frac{1}{\eta_{\psi}}-\frac{d^{2}\log\Gamma(\eta_{\psi})}{(d\eta_{\psi})^{2}}],
\]

\end_inset

where 
\begin_inset Formula $\frac{d^{2}\log\Gamma(\eta_{\psi})}{(d\eta_{\psi})^{2}}$
\end_inset

 is a trigamma function.
 We use the first derivative to obtain the conditional posterior mode 
\begin_inset Formula $\hat{\eta}_{\psi}$
\end_inset

.
 And then,
 the hessian is evaluated to derive the variance of the proposal distribution.
 The proposal is a student-t distribution of
\begin_inset Formula 
\begin{align*}
t_{15}(\hat{\eta}_{\psi},(-\frac{\partial^{2}\log p(\eta_{\psi}|\mathcal{I}_{T},\theta\backslash\{\eta_{\psi}\})}{(\partial\eta_{\psi})^{2}})^{-1}|_{\eta_{\psi}=\hat{\eta}_{\psi}}).
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Other transition equation parameters 
\begin_inset Formula $\{\phi_{i},\sigma_{\mathcal{FF},i}^{2}\}_{i=d_{\mathbb{Q}}+1}^{d_{\mathbb{P}}}$
\end_inset


\end_layout

\begin_layout Standard
The full-conditional distribution is 
\begin_inset Formula 
\begin{align*}
 & \Pi_{i=d_{\mathbb{Q}}+1}^{d_{\mathbb{P}}}\mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T})\times p(\{\sigma_{\mathcal{FF},i}^{2}\}_{i=d_{\mathbb{Q}}+1}^{d_{\mathbb{P}}}|\nu_{0},\Omega_{0})\\
\times & p(\{\phi_{i}\}_{i=d_{\mathbb{Q}}+1}^{d_{\mathbb{P}}}|p,\{\sigma_{\mathcal{FF},i}^{2}\}_{i=d_{\mathbb{Q}}+1}^{d_{\mathbb{P}}},q,\nu_{0},\Omega_{0},\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}})
\end{align*}

\end_inset

For 
\begin_inset Formula $i\in\{d_{\mathbb{Q}}+1,\cdots,d_{\mathbb{P}}\}$
\end_inset

,
 the kernel is 
\begin_inset Formula 
\[
\mathcal{N}(y_{\phi_{i}}|X_{\phi_{i}}\phi_{i},\sigma_{\mathcal{FF},i}^{2}I_{T})\times\mathcal{N}(\phi_{i}|m_{i},\sigma_{\mathcal{FF},i}^{2}diag[V_{i}])\times\mathcal{IG}(\sigma_{\mathcal{FF},i}^{2}|\frac{\nu_{0}+i-d_{\mathbb{P}}}{2},\frac{\Omega_{0,(i)}}{2}),
\]

\end_inset

so it is the Normal-InverseGamma-Normal-InverseGamma update.
\end_layout

\begin_layout Standard
In the case of 
\begin_inset Formula 
\begin{align*}
y|\beta\text{,}\sigma^{2} & =X\beta+\mathcal{N}(O,\sigma^{2}I_{T}),\\
\beta|\sigma^{2} & \backsim\mathcal{N}(\beta_{0},\sigma^{2}B_{0}),\\
\sigma^{2} & \backsim\mathcal{IG}(\alpha_{0},\delta_{0}),
\end{align*}

\end_inset

the posterior distribution is 
\begin_inset Formula 
\begin{align*}
\beta|\sigma^{2},y & \backsim\mathcal{N}(\beta_{1},\sigma^{2}B_{1}),\\
\sigma^{2}|y & \backsim\mathcal{IG}(\alpha_{0}+\frac{T}{2},\delta_{1}),
\end{align*}

\end_inset

where 
\begin_inset Formula $B_{1}=(B_{0}^{-1}+X^{\prime}X)^{-1}$
\end_inset

,
 
\begin_inset Formula $\beta_{1}=B_{1}(B_{0}^{-1}\beta_{0}+X^{\prime}y)$
\end_inset

,
 and 
\begin_inset Formula $\delta_{1}=\delta_{0}+(y^{\prime}y+\beta_{0}^{\prime}B_{0}^{-1}\beta_{0}-\beta_{1}^{\prime}B_{1}^{-1}\beta_{1})/2.$
\end_inset


\end_layout

\begin_layout Subsubsection
Sparsity parameter 
\begin_inset Formula $\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}}$
\end_inset

,
 and 
\begin_inset Formula $\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}}$
\end_inset


\end_layout

\begin_layout Standard
The kernel is 
\begin_inset Formula 
\begin{align*}
 & p(\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}}|\mathcal{I}_{T},\theta\backslash\{\{\{\psi_{l,ij}\}_{l=1}^{p}\}_{i,j=1}^{d_{\mathbb{P}}},\{\psi_{0,i}\}_{i=1}^{d_{\mathbb{P}}}\})\\
\propto & \Pi_{l=1}^{p}\Pi_{i,j=1}^{d_{\mathbb{P}}}[\mathcal{N}(\phi_{i,(1+(l-1)d_{\mathbb{P}}+j)}|m_{i,(1+(l-1)d_{\mathbb{P}}+j)},\psi_{l,ij}\sigma_{\mathcal{FF},i}^{2}\frac{q_{slope}}{l^{q_{lag}}}\frac{(\nu_{0}-d_{\mathbb{P}}-1)}{\Omega_{0,(j)}})\\
 & \times\mathcal{G}(\psi_{l,ij}|\eta_{\psi},\eta_{\psi})]\times\Pi_{i=1}^{d_{\mathbb{P}}}[\mathcal{N}(\phi_{i,(1)}|m_{i,(1)},\psi_{0,i}\sigma_{\mathcal{FF},i}^{2}q_{intercept})\times\mathcal{G}(\psi_{0,i}|\eta_{\psi},\eta_{\psi})].
\end{align*}

\end_inset

Each component in the brackets are updated to distributions of the Generalized Inverse Gaussian (GIG).
 In general notation,
 it is 
\begin_inset Formula 
\begin{align*}
\phi & \backsim\mathcal{N}(m,\psi V),\\
\psi & \backsim\mathcal{G}(\eta,\eta),
\end{align*}

\end_inset

the corresponding kernel is 
\begin_inset Formula 
\[
p(\psi|\phi)\propto\psi^{\eta-\frac{1}{2}-1}\exp[-\frac{1}{2}(2\eta\psi+\psi^{-1}\frac{(\phi-m)^{2}}{V})],
\]

\end_inset

that is a kernel of 
\begin_inset Formula 
\[
p(\psi|\phi)\propto\mathcal{GIG}(\eta-\frac{1}{2},2\eta,\frac{(\phi-m)^{2}}{V}).
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Pricing error 
\begin_inset Formula $\Sigma_{\mathcal{O}}$
\end_inset


\end_layout

\begin_layout Standard
The full-conditional distribution is 
\begin_inset Formula 
\begin{align*}
 & \Pi_{i=1}^{N-d_{\mathbb{Q}}}[\{\Pi_{t=1}^{T}\mathcal{N}(\mathcal{O}_{t,(i)}|\mathcal{A}_{\mathcal{P},(i)}+\mathcal{B}_{\mathcal{P},(i,:)}(\mathcal{P}_{t}-c),\sigma_{\mathcal{O},i}^{2})\}\mathcal{IG}(\sigma_{\mathcal{O},i}^{2}|2,\gamma_{i})]\\
= & \Pi_{i=1}^{N-d_{\mathbb{Q}}}[\mathcal{N}(\underbrace{\begin{bmatrix}\mathcal{O}_{1,(i)}\\
\mathcal{O}_{2,(i)}\\
\vdots\\
\mathcal{O}_{T,(i)}
\end{bmatrix}}_{y_{\sigma_{\mathcal{O},i}^{2}}}|\underbrace{\begin{bmatrix}\mathcal{A}_{\mathcal{P},(i)}+\mathcal{B}_{\mathcal{P},(i,:)}(\mathcal{P}_{1}-c)\\
\mathcal{A}_{\mathcal{P},(i)}+\mathcal{B}_{\mathcal{P},(i,:)}(\mathcal{P}_{2}-c)\\
\vdots\\
\mathcal{A}_{\mathcal{P},(i)}+\mathcal{B}_{\mathcal{P},(i,:)}(\mathcal{P}_{T}-c)
\end{bmatrix}}_{m_{\sigma_{\mathcal{O},i}^{2}}},\sigma_{\mathcal{O},i}^{2}I_{T})\mathcal{IG}(\sigma_{\mathcal{O},i}^{2}|2,\gamma_{i})].
\end{align*}

\end_inset

 Therefore,
 we can update the components in the brackets using the InverseGamma-InverseGamma update.
 For each 
\begin_inset Formula $i$
\end_inset

,
\begin_inset Formula 
\[
p(\sigma_{\mathcal{O},i}^{2}|\mathcal{I}_{T},\theta\backslash\{\Sigma_{\mathcal{O}}\})=\mathcal{IG}(\sigma_{\mathcal{O},i}^{2}|2+\frac{T}{2},\gamma_{i}+\frac{(y_{\sigma_{\mathcal{O},i}^{2}}-m_{\sigma_{\mathcal{O},i}^{2}})^{\prime}(y_{\sigma_{\mathcal{O},i}^{2}}-m_{\sigma_{\mathcal{O},i}^{2}})}{2}).
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Population measurement error 
\begin_inset Formula $\gamma$
\end_inset


\end_layout

\begin_layout Standard
The corresponding kernel is 
\begin_inset Formula 
\begin{align*}
 & \Pi_{i=1}^{N-d_{\mathbb{Q}}}p(\sigma_{\mathcal{O},i}^{2}|\gamma_{i})p(\gamma_{i})\\
= & \Pi_{i=1}^{N-d_{\mathbb{Q}}}\mathcal{IG}(\sigma_{\mathcal{O},i}^{2}|2,\gamma_{i})\mathcal{G}(\gamma_{i}|1,\bar{\gamma}).
\end{align*}

\end_inset

Therefore,
 there is a posterior independence.
 For each 
\begin_inset Formula $i$
\end_inset

,
\begin_inset Formula 
\begin{align*}
 & \mathcal{IG}(\sigma_{\mathcal{O},i}^{2}|2,\gamma_{i})\mathcal{G}(\gamma_{i}|1,\bar{\gamma})\\
= & \frac{\gamma_{i}^{2}}{\Gamma(2)}(\sigma_{\mathcal{O},i}^{2})^{-(2+1)}\exp[-\sigma_{\mathcal{O},i}^{-2}\gamma_{i}]\frac{\bar{\gamma}}{\Gamma(1)}\exp[-\bar{\gamma}\gamma_{i}],\\
\propto & \gamma_{i}^{3-1}\exp[-(\sigma_{\mathcal{O},i}^{-2}+\bar{\gamma})\gamma_{i}].
\end{align*}

\end_inset

Therefore,
 
\begin_inset Formula 
\[
p(\gamma_{i}|\mathcal{I}_{T},\theta\backslash\{\gamma_{i}\}_{i=1}^{N-d_{\mathbb{Q}}})=\mathcal{G}(3,\frac{1}{\sigma_{\mathcal{O},i}^{2}}+\bar{\gamma}).
\]

\end_inset


\end_layout

\begin_layout Subsection
Empirical Bayes and the marginal likelihood
\end_layout

\begin_layout Standard
Hyperparameters,
 
\begin_inset Formula $p$
\end_inset

,
 
\begin_inset Formula $\nu_{0}$
\end_inset

,
 
\begin_inset Formula $\Omega_{0}$
\end_inset

,
 and 
\begin_inset Formula $q$
\end_inset

,
 should be calibrated.
 We calibrate them in the perspective of the empirical Bayes.
 We maximize the marginal likelihood for the 
\begin_inset Formula $\mathbb{P}$
\end_inset

-transition equation by calibrating 
\begin_inset Formula $p$
\end_inset

,
 
\begin_inset Formula $\nu_{0}$
\end_inset

,
 
\begin_inset Formula $\Omega_{0}$
\end_inset

,
 and 
\begin_inset Formula $q$
\end_inset

,
 given 
\begin_inset Formula $\forall\{\psi_{l,ij},\psi_{0,i}\}=1$
\end_inset

.
 The log marginal likelihood is 
\begin_inset Formula 
\begin{align*}
 & \sum_{i=1}^{d_{\mathbb{P}}}\log\mathcal{N}(y_{\phi_{i}}|p,\nu_{0},\Omega_{0},q,\psi_{l,ij},\psi_{0,i})\\
= & -\frac{Td_{\mathbb{P}}}{2}\log[2\pi]+\sum_{i=1}^{d_{\mathbb{P}}}\{-\frac{1}{2}(\sum_{j=1}^{\#\phi_{i}}\log[V_{i,(j)}]+\log\det[K_{\phi_{i}}])\\
 & +\log\Gamma(\nu_{i}+\frac{T}{2})+\nu_{i}\log S_{i}-\log\Gamma(\nu_{i})-(\nu_{i}+\frac{T}{2})\log[\hat{S}_{i}]\}
\end{align*}

\end_inset


\begin_inset CommandInset citation
LatexCommand citep
key "chan2022asymmetric"
literal "true"

\end_inset

.
 Here,
 
\begin_inset Formula $\nu_{i}=(\nu_{0}+i-d_{\mathbb{P}})/2$
\end_inset

,
 
\begin_inset Formula $S_{i}=\Omega_{0,(i)}/2$
\end_inset

,
 
\begin_inset Formula $K_{\phi_{i}}=diag[V_{i}]^{-1}+X_{\phi_{i}}^{\prime}X_{\phi_{i}}$
\end_inset

,
 
\begin_inset Formula $\hat{\phi}_{i}=K_{\phi_{i}}^{-1}(diag[V_{i}]^{-1}m_{i}+X_{\phi_{i}}^{\prime}y_{\phi_{i}})$
\end_inset

,
 
\begin_inset Formula $\hat{S}_{i}=S_{i}+(y_{\phi_{i}}^{\prime}y_{\phi_{i}}+m_{i}^{\prime}diag[V_{i}]^{-1}m_{i}-\hat{\phi}_{i}^{\prime}K_{\phi_{i}}\hat{\phi}_{i})/2.$
\end_inset


\end_layout

\begin_layout Standard
When we maximize the marginal likelihood,
 we constrain the maximum Sharpe ratio.
 To do that,
 we use the below proposition.
\end_layout

\begin_layout Proposition
\begin_inset CommandInset label
LatexCommand label
name "prop:mSR"

\end_inset

In our specification,
 the second-order Taylor series approximates the conditional expectation for the maximum Sharpe ratio as
\begin_inset Formula 
\begin{align*}
 & \mathbb{E}[\sqrt{\lambda_{t}'\lambda_{t}}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}]\\
\approx & \frac{\sqrt{\pi}}{2}\left[\frac{(tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}})^{0.5}}{\Gamma(1.5)}+\frac{(tr[B_{\lambda_{t}}]+\beta_{\lambda_{t}}^{\prime}\beta_{\lambda_{t}})^{-1.5}(2tr[B_{\lambda_{t}}^{2}]+4\beta_{\lambda_{t}}^{\prime}B_{\lambda_{t}}\beta_{\lambda_{t}})}{2\Gamma(-0.5)}\right].
\end{align*}

\end_inset

If we assume 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

 as a diagonal,
 the expressions for 
\begin_inset Formula $\beta_{\lambda_{t}}$
\end_inset

 and 
\begin_inset Formula $B_{\lambda_{t}}$
\end_inset

 get simplified.
 Also,
 under diagonal 
\begin_inset Formula $\Omega_{\mathcal{FF}}$
\end_inset

,
\end_layout

\begin_layout Proposition
\begin_inset Formula 
\begin{align*}
 & \mathbb{E}[\sqrt{(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})^{\prime}\Omega_{\mathcal{FF}}^{-1}(K_{\mathcal{F}}^{\mathbb{P}}-K_{\mathcal{F}}^{\mathbb{Q}})}|\{\mathcal{F}_{t}\}_{t=1}^{T},\Omega_{\mathcal{FF}},k_{\infty}^{\mathbb{Q}},\kappa^{\mathbb{Q}}]\\
\approx & \frac{\sqrt{\pi}}{2}\left[\frac{(tr[B_{const}]+\beta_{const}^{\prime}\beta_{const})^{0.5}}{\Gamma(1.5)}+\frac{(tr[B_{const}]+\beta_{const}^{\prime}\beta_{const})^{-1.5}(2tr[B_{const}^{2}]+4\beta_{const}^{\prime}B_{const}\beta_{const})}{2\Gamma(-0.5)}\right].
\end{align*}

\end_inset


\end_layout

\begin_layout Proof
See Appendix 
