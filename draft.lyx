\begin_layout Subsection
Expected Return-Beta Representation
\end_layout

\begin_layout Standard
Market prices of risks 
\begin_inset Formula $\lambda_{t}$
\end_inset

 is a price for 
\begin_inset Formula 
\begin{align}
v_{\mathcal{F},t}^{\mathbb{P}} & \equiv\Omega_{\mathcal{FF}}^{-1/2}\epsilon_{\mathcal{F},t}^{\mathbb{P}}\backsim\mathcal{N}(O_{d_{\mathbb{P}}\times1},I_{d_{\mathbb{P}}}).\label{eq:v}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
To show that,
 we first define the holding-period return,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
hr_{\tau,t+1} & \equiv\ln P_{\tau-1,t+1}-\ln P_{\tau,t},\nonumber \\
 & =-a_{\tau-1}-b_{\tau-1}^{\prime}X_{t+1}+a_{\tau}+b_{\tau}^{\prime}X_{t}\nonumber \\
 & =-a_{\tau-1}-\underbrace{\begin{bmatrix}b_{\tau-1}^{\prime} & O_{1\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\end{bmatrix}}_{b_{F,\tau-1}^{\prime}}F_{t+1}+a_{\tau}+b_{F,\tau}^{\prime}F_{t},\nonumber \\
 & =-a_{\tau-1}-b_{F,\tau-1}^{\prime}(\mathcal{T}_{0,\mathcal{F}}+\mathcal{T}_{1,\mathcal{F}}\mathcal{F}_{t+1})+a_{\tau}+b_{F,\tau}^{\prime}(\mathcal{T}_{0,\mathcal{F}}+\mathcal{T}_{1,\mathcal{F}}\mathcal{F}_{t}),\nonumber \\
 & =a_{\tau}-a_{\tau-1}+(b_{F,\tau}^{\prime}-b_{F,\tau-1}^{\prime})\mathcal{T}_{0,\mathcal{F}}-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}\mathcal{F}_{t+1}+b_{F,\tau}^{\prime}\mathcal{T}_{1,\mathcal{F}}\mathcal{F}_{t}.\label{eq:aux2}
\end{align}

\end_inset

By 
\begin_inset Formula $\mathbb{P}$
\end_inset

-dynamics 
\begin_inset Formula $\mathcal{F}_{t+1}=K_{\mathcal{F}}^{\mathbb{P}}+\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t+1-l}+\epsilon_{\mathcal{F},t+1}^{\mathbb{P}},$
\end_inset


\begin_inset Formula 
\begin{align}
hr_{\tau,t+1}= & a_{\tau}-a_{\tau-1}+(b_{F,\tau}^{\prime}-b_{F,\tau-1}^{\prime})\mathcal{T}_{0,\mathcal{F}}+b_{F,\tau}^{\prime}\mathcal{T}_{1,\mathcal{F}}\mathcal{F}_{t}\nonumber \\
 & -b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}K_{\mathcal{F}}^{\mathbb{P}}-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}\sum_{l=1}^{p}G_{\mathcal{FF},l}^{\mathbb{P}}\mathcal{F}_{t+1-l}-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}\Omega_{\mathcal{FF}}^{1/2}v_{\mathcal{F},t+1}^{\mathbb{P}}.\label{eq:hr}
\end{align}

\end_inset

In the case of the beta,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
Cov_{t}^{\mathbb{P}}[hr_{\tau,t+1},v_{\mathcal{F},t+1}^{\mathbb{P}}] & =\mathbb{E}_{t}^{\mathbb{P}}[hr_{\tau,t+1}v_{\mathcal{F},t+1}^{\mathbb{P}\prime}]-\mathbb{E}_{t}^{\mathbb{P}}[hr_{\tau,t+1}]\mathbb{E}_{t}^{\mathbb{P}}[v_{\mathcal{F},t+1}^{\mathbb{P}\prime}],\nonumber \\
 & =\mathbb{E}_{t}^{\mathbb{P}}[-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}\Omega_{\mathcal{FF}}^{1/2}v_{\mathcal{F},t+1}^{\mathbb{P}}v_{\mathcal{F},t+1}^{\mathbb{P}\prime}],\nonumber \\
 & =-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}\Omega_{\mathcal{FF}}^{1/2},\label{eq:aux3}\\
 & =-\begin{bmatrix}b_{\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{P}} & O_{1\times(d_{\mathbb{P}}-d_{\mathbb{Q}})}\end{bmatrix}\Omega_{\mathcal{FF}}^{1/2},\nonumber 
\end{align}

\end_inset

Therefore,
 the expected excess return is
\begin_inset Formula 
\begin{align*}
\mathbb{E}_{t}^{\mathbb{P}}[hr_{\tau,t+1}]-\mathbb{E}_{t}^{\mathbb{Q}}[hr_{\tau,t+1}] & =-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}(\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{F}_{t+1}]-\mathbb{E}_{t}^{\mathbb{Q}}[\mathcal{F}_{t+1}])\text{ by equation \eqref{eq:aux2},}\\
 & =-b_{F,\tau-1}^{\prime}\mathcal{T}_{1,\mathcal{F}}\Omega_{\mathcal{FF}}^{1/2}\Omega_{\mathcal{FF}}^{-1/2}(\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{F}_{t+1}]-\mathbb{E}_{t}^{\mathbb{Q}}[\mathcal{F}_{t+1}]),\\
 & =Cov_{t}^{\mathbb{P}}[hr_{\tau,t+1},v_{\mathcal{F},t+1}^{\mathbb{P}}]\lambda_{t}\text{ by equations \eqref{eq:aux3} and \eqref{eq:aux4}},\\
 & =\sum_{i=1}^{d_{\mathbb{P}}}\underbrace{\frac{Cov_{t}^{\mathbb{P}}[hr_{\tau,t+1},v_{\mathcal{F},t+1,(i)}^{\mathbb{P}}]}{Var_{t}^{\mathbb{P}}[v_{\mathcal{F},t+1,(i)}^{\mathbb{P}}]}}_{beta}\lambda_{t,(i)}.
\end{align*}

\end_inset

Our beta representation shows that 
\begin_inset Formula $\lambda_{t}$
\end_inset

 is a price for 
\begin_inset Formula $v_{\mathcal{F},t+1}^{\mathbb{P}}$
\end_inset

.
\end_layout

\begin_layout Subsection
Sharpe Ratio
\end_layout

\begin_layout Standard
We define the Sharpe ratio as 
\begin_inset Formula 
\begin{align*}
SR_{\tau,t} & :=\frac{\mathbb{E}_{t}^{\mathbb{P}}[xr_{\tau,t+1}]+\frac{1}{2}Var_{t}^{\mathbb{P}}[xr_{\tau,t+1}]}{Std_{t}^{\mathbb{P}}[xr_{\tau,t+1}]},
\end{align*}

\end_inset

where 
\begin_inset Formula $xr_{\tau,t+1}:=\ln P_{\tau-1,t+1}-\ln P_{\tau,t}-r_{t}.$
\end_inset

 By equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:LOP"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

,
 
\begin_inset Formula 
\begin{align}
1 & =\mathbb{E}_{t}^{\mathbb{P}}[\mathcal{M}_{t+1}^{\mathbb{P}}\frac{P_{\tau-1,t+1}}{P_{\tau,t}}],\nonumber \\
1 & =\mathbb{E}_{t}^{\mathbb{P}}[\exp[\ln\mathcal{M}_{t+1}^{\mathbb{P}}+(xr_{\tau,t+1}+r_{t})]],\nonumber \\
0 & =\mathbb{E}_{t}^{\mathbb{P}}\ln[\mathcal{M}_{t+1}^{\mathbb{P}}]+\mathbb{E}_{t}^{\mathbb{P}}[xr_{\tau,t+1}]+r_{t}\nonumber \\
 & +\frac{1}{2}Var_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}}]+\frac{1}{2}Var_{t}^{\mathbb{P}}[xr_{\tau,t+1}]+Cov_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}},xr_{\tau,t+1}]).\label{eq:aux1}
\end{align}

\end_inset

When 
\begin_inset Formula $\tau=1$
\end_inset

,
\begin_inset Formula 
\begin{align*}
0 & =\mathbb{E}_{t}^{\mathbb{P}}\ln[\mathcal{M}_{t+1}^{\mathbb{P}}]+r_{t}+\frac{1}{2}Var_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}}].
\end{align*}

\end_inset

Therefore,
 we can re-write equation (
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:aux1"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

) as
\begin_inset Formula 
\begin{align*}
0 & =\mathbb{E}_{t}^{\mathbb{P}}[xr_{\tau,t+1}]+\frac{1}{2}Var_{t}^{\mathbb{P}}[xr_{\tau,t+1}]+Cov_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}},xr_{\tau,t+1}]),
\end{align*}

\end_inset


\begin_inset Formula 
\[
\mathbb{E}_{t}^{\mathbb{P}}[xr_{\tau,t+1}]+\frac{1}{2}Var_{t}^{\mathbb{P}}[xr_{\tau,t+1}]=-Cov_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}},xr_{\tau,t+1}]).
\]

\end_inset

It simplifies the definition of the Sharpe ratio to
\begin_inset Formula 
\begin{align*}
SR_{\tau,t} & =\frac{-Cov_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}},xr_{\tau,t+1}])}{Std_{t}^{\mathbb{P}}[xr_{\tau,t+1}]},\\
 & =-Corr_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}},xr_{\tau,t+1}])Std_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}}],\\
 & \leq Std_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}}].
\end{align*}

\end_inset

We define 
\begin_inset Formula $Std_{t}^{\mathbb{P}}[\ln\mathcal{M}_{t+1}^{\mathbb{P}}]$
\end_inset

 as the maximum Sharpe ratio.
\end_layout

